.kill-index {
  background: #ffffff;
  min-height: 540px;
}
.kill-index .tab {
  padding-left: 10px;
  padding-right: 15px;
  overflow: hidden;
  margin-bottom: 20px;
}
.kill-index .tab a:hover,
.kill-index .tab a:active,
.kill-index .tab a:focus {
  color: #333333;
}
.kill-index .tab a {
  width: 111px;
  text-align: center;
  padding: 19.5px 0;
  position: relative;
}
.kill-index .tab a.active {
  color: #b69b69;
}
.kill-index .tab a.active:after {
  position: absolute;
  content: "";
  height: 2px;
  width: 100%;
  background: #b2955e;
  left: 0;
  bottom: 0;
}
.kill-index .tab a.add {
  width: 134px;
}
.kill-index .tab a.add span {
  height: 33px;
  line-height: 33px;
  background: #f3f3f3;
  font-size: 14px;
  display: block;
  color: #b2955e;
  border: 1px solid #ededed;
  border-radius: 5px;
}
.kill-index .kill-index-container {
  padding: 0 15px;
}
.kill-index .kill-index-container .search {
  overflow: hidden;
  box-sizing: content-box;
  background: #f3f3f3;
  padding: 10px;
  height: 28px;
  line-height: 28px;
}
.kill-index .kill-index-container .search .page-title {
  margin-left: 35px;
  margin-right: 15px;
}
.kill-index .kill-index-container .search .select-wrap {
  background: #ffffff;
  width: 67px;
  overflow: hidden;
  border-radius: 4px;
}
.kill-index .kill-index-container .search .select {
  width: 67px;
  height: 28px;
  line-height: 28px;
  margin-right: 70px;
}
.kill-index .kill-index-container .search .key {
  height: 28px;
  line-height: 28px;
  background: #ffffff;
  border-radius: 4px;
  width: 203px;
  overflow: hidden;
  padding: 0 8px;
}
.kill-index .kill-index-container .search .key .input input {
  height: 28px;
  line-height: 28px;
  width: 159px;
}
.kill-index .kill-index-container .search .key a {
  width: 26px;
  height: 28px;
  background: url("data:image/png;base64,") center center no-repeat;
  background-size: 50% 50%;
}
.kill-index .kill-index-container .search .go-search {
  height: 28px;
  line-height: 28px;
  background: #b69b69;
  margin-left: 12px;
  color: #ffffff;
  width: 80px;
  vertical-align: top;
  text-align: center;
}
.kill-index .kill-index-container .table table {
  width: 100%;
}
.kill-index .kill-index-container .table table th {
  padding-top: 36px;
  padding-bottom: 14px;
  color: #808080;
  font-size: 12px;
  text-align: center;
  font-weight: normal;
  border-bottom: 1px solid #ccd6e1;
}
.kill-index .kill-index-container .table table .operation {
  text-align: left;
  width: 300px;
}
.kill-index .kill-index-container .table table tr:nth-child(2n) {
  background: #ffffff;
}
.kill-index .kill-index-container .table table tr:nth-child(2n+1) {
  background: #eff2f7;
}
.kill-index .kill-index-container .table table tr:nth-child(1) {
  background: #ffffff;
}
.kill-index .kill-index-container .table table td {
  height: 74px;
  vertical-align: middle;
  color: #333333;
  font-size: 12px;
  text-align: center;
  padding: 0 20px;
}
.kill-index .kill-index-container .table table td .btn {
  display: inline-block;
  box-sizing: content-box;
  padding: 0;
  height: 28px;
  line-height: 28px;
  color: #b69b69;
  border: 1px solid #b69b69;
  text-align: center;
  width: 77px;
  font-size: 14px;
  margin-right: 6px;
}
.kill-index .kill-index-container .table table td .btn.placeholder {
  visibility: hidden;
}
.kill-index .kill-index-container .table table td .btn.disabled {
  border: 1px solid #ffffff;
  color: #333333;
}
.kill-index .kill-index-container .table table td .btn:last-child {
  margin-right: 0;
}
.kill-index .kill-index-container .pagination {
  height: 24px;
  line-height: 24px;
  margin-top: 18px;
  padding-bottom: 18px;
  color: #bfbfbf;
  width: 100%;
}
.kill-index .kill-index-container .pagination span {
  color: #808080;
}
.kill-index .kill-index-container .pagination .page {
  margin-right: 10px;
}
.kill-index .kill-index-container .pagination .page a {
  height: 23px;
  line-height: 23px;
  text-align: center;
}
.kill-index .kill-index-container .pagination .page .last,
.kill-index .kill-index-container .pagination .page .next,
.kill-index .kill-index-container .pagination .page .go {
  width: 23px;
  border: 1px solid #eeeeee;
  border-radius: 2.5px;
}
.kill-index .kill-index-container .pagination .page .last {
  margin-right: 5px;
  background: url("data:image/png;base64,") center center no-repeat;
  background-size: 4.5px 8px;
}
.kill-index .kill-index-container .pagination .page .next {
  margin-left: 5px;
  background: url("data:image/png;base64,") center center no-repeat;
  background-size: 4.5px 8px;
}
.kill-index .kill-index-container .pagination .page .go {
  font-size: 12px;
  color: #333333;
}
.kill-index .kill-index-container .pagination .page .tips {
  height: 24px;
  line-height: 24px;
  font-size: 10px;
  color: #555555;
  margin-right: 5px;
  margin-left: 5px;
}
.kill-index .kill-index-container .pagination .page .go-to-input {
  width: 24px;
  height: 23px;
  border: 1px solid #eeeeee;
  padding: 0;
  text-align: center;
  color: #333333;
  margin: 0 5px 0 0;
}
.kill-index .kill-index-container .pagination .page .current {
  border: 1px solid #eeeeee;
  border-radius: 2.5px;
}
.kill-index .kill-index-container .pagination .page .current a {
  box-sizing: content-box;
  border-right: 1px solid #eeeeee;
  padding: 0 10px;
  font-size: 12px;
  color: #808080;
}
.kill-index .kill-index-container .pagination .page .current a.active {
  background: #b2955e;
  color: #ffffff;
}
.kill-index .kill-index-container .pagination .page .current a:last-child {
  border-right: none;
}
.kill-edit {
  position: relative;
}
.kill-edit #week,
.kill-edit #selectTime,
.kill-edit #limitPeopleNumber,
.kill-edit #peopleNumberTips,
.kill-edit #errorBox,
.kill-edit #goodsInfo {
  display: none;
}
.kill-edit .error {
  background: #fff4e2;
  box-sizing: content-box;
  height: 15px;
  line-height: 15px;
  padding: 17px 37px 17px 28px;
  border: 1px solid #fde7ba;
  border-radius: 5px;
  color: #666;
  margin-bottom: 10px;
}
.kill-edit .error .icon {
  width: 15px;
  height: 15px;
  background: url("data:image/png;base64,") center center no-repeat;
  background-size: 100%;
  margin-right: 20px;
}
.kill-edit .error .close {
  width: 15px;
  height: 15px;
  background: url("data:image/png;base64,") center center no-repeat;
  background-size: 100%;
  position: relative;
  z-index: 999;
}
.kill-edit .kill-edit-container {
  padding: 30px;
  background: #ffffff;
}
.kill-edit .kill-edit-container #keyWord {
