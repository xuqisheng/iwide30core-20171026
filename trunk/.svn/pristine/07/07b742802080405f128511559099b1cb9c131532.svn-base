.kill-edit .kill-edit-container .kill-content ul li .formdate.start-time {
  width: 120px;
  margin-right: 37px;
}
.kill-edit .kill-edit-container .kill-content .save {
  text-align: center;
}
.kill-edit .kill-edit-container .kill-content .save a {
  display: inline-block;
  width: 148px;
  height: 44px;
  line-height: 44px;
  color: #808080;
  border-radius: 5px;
  border: 1px solid #bfbfbf;
  margin-right: 7px;
}
.kill-edit .kill-edit-container .kill-content .save a.active {
  background: #b69b69;
  border: 1px solid #b69b69;
  color: #ffffff;
}
.kill-edit.disabled:after {
  content: "";
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 99999;
}
.kill-edit.disabled input[type="radio"]:checked {
  border: 1px solid #bfbfbf;
}
.kill-edit.disabled input[type="radio"]:checked::after {
  content: "";
  position: absolute;
  width: 50%;
  height: 50%;
  left: 25%;
  top: 25%;
  background: #bfbfbf;
  border-radius: 100%;
}
.kill-layer {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}
.kill-layer .layer-content {
  position: absolute;
  min-width: 343px;
  border-radius: 5px;
  left: 50%;
  top: 50%;
  padding-top: 22.5px;
  padding-bottom: 40px;
  -webkit-transform: translate(-50%, -50%);
  -moz-transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background: #ffffff;
}
.kill-layer .layer-content .close {
  position: absolute;
  width: 20px;
  height: 20px;
  background: url("data:image/png;base64,") center center no-repeat;
  background-size: 50% 50%;
  opacity: 1;
  right: 24.5px;
  top: 24px;
  z-index: 99;
}
.kill-layer .layer-content .title {
  text-align: center;
  position: relative;
}
.kill-layer .layer-content .tips-content {
  padding: 53px 40px 53px 40px;
  text-align: center;
}
.kill-layer .layer-content .store {
  margin: 0 auto;
  padding: 40px 40px 30px 40px;
}
.kill-layer .layer-content .store li {
  color: #333333;
  height: 30px;
  line-height: 30px;
  margin-bottom: 10px;
}
.kill-layer .layer-content .store li .title {
  width: 84px;
  text-align: right;
  color: #808080;
  margin-right: 25px;
}
.kill-layer .layer-content .store li .number {
  height: 29px;
  line-height: 29px;
  width: 73px;
  border: 1px solid #eeeeee;
}
.kill-layer .layer-content .store li .unit {
  height: 30px;
  line-height: 30px;
  margin-left: 8px;
}
.kill-layer .layer-content .btn {
  box-sizing: content-box;
  display: block;
  padding: 0;
  width: 152px;
  margin: 0 auto;
  height: 40px;
  line-height: 40px;
  background: #b69b69;
  color: #ffffff;
  text-align: center;
}
