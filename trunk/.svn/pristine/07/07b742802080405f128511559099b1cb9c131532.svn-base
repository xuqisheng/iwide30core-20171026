  width: 100%;
  top: 20px;
  left: 0;
  height: 15px;
}
.kill-edit .kill-edit-container .main-title .line:after {
  position: absolute;
  content: '';
  width: 100%;
  height: 2px;
  left: 0;
  top: 50%;
  margin-top: -1px;
  background: #f7f7f7;
}
.kill-edit .kill-edit-container .kill-content {
