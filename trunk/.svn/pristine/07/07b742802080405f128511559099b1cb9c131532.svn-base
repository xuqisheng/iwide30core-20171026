* {
  margin: 0;
  padding: 0;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.f28 {
  font-size: 14px;
}
.f24 {
  font-size: 12px;
}
.f32 {
  font-size: 16px;
}
.c333 {
  color: #333333;
}
a {
  text-decoration: none;
}
li {
  list-style: none;
}
.clearfix:after {
  content: "";
  height: 0;
  clear: both;
  overflow: hidden;
}
input,
select {
  border: none;
  outline: none;
  -webkit-appearance: none;
  -moz-appearance: none;
  -ms-appearance: none;
  font-size: 12px;
  padding: 0 9px;
  vertical-align: top;
  border-radius: 4px;
}
select {
  background: url("data:image/png;base64,") no-repeat scroll right center transparent;
  background-size: 10px;
  background-position-x: 90%;
}
input[type="radio"] {
  width: 14px !important;
  height: 14px !important;
  padding: 0px !important;
  border: 1px solid #e1e1e1;
  border-radius: 100%;
  vertical-align: middle;
  cursor: pointer;
  position: relative;
  box-shadow: none;
}
input[type="radio"]:checked {
  border: 1px solid #b69b69;
}
input[type="radio"]:checked::after {
  content: "";
  position: absolute;
  width: 50%;
  height: 50%;
  left: 25%;
  top: 25%;
  background: #b69b69;
  border-radius: 100%;
}
input[type="checkbox"] {
  width: 16px !important;
  height: 15.5px !important;
  padding: 0 !important;
  border: 1px solid #e1e1e1;
  border-radius: 0;
  position: relative;
}
input[type="checkbox"]:checked {
  border: 1px solid #b69b69;
}
input[type="checkbox"]:checked::after {
  content: "";
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: url("data:image/png;base64,") center center no-repeat;
  background-size: 100% 100%;
}
input::-webkit-input-placeholder {
  color: #bfbfbf;
}
input:-moz-placeholder {
  color: #bfbfbf;
}
input::-moz-placeholder {
  color: #bfbfbf;
}
input:-ms-input-placeholder {
  color: #bfbfbf;
}
input[type="reset"]::-moz-focus-inner,
input[type="button"]::-moz-focus-inner,
input[type="submit"]::-moz-focus-inner,
input[type="file"] > input[type="button"]::-moz-focus-inner {
  border: none;
  padding: 0;
}
