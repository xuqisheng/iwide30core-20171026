@keyframes lightSpeedIn {
  from {
    -webkit-transform: translate3d(-100%, 0, 0) skewX(-30deg);
    transform: translate3d(-100%, 0, 0) skewX(-30deg);
    opacity: 0;
  }

  60% {
    -webkit-transform: skewX(20deg);
    transform: skewX(20deg);
    opacity: 1;
  }

  80% {
    -webkit-transform: skewX(-5deg);
    transform: skewX(-5deg);
    opacity: 1;
  }

  to {
    -webkit-transform: none;
    transform: none;
    opacity: 1;
  }
}

.lightSpeedIn {
  -webkit-animation: lightSpeedIn ease-out 1s forwards;
  animation: lightSpeedIn ease-out 1s forwards;
}
.gouwu_texiao {
    width: 100%;
    height: 100%;
    position: absolute;
    border-radius: 100%;
    top: 0px;
    left: 0px;
    background-color: #eb50a9;
    animation: tx 1s;
    -webkit-animation: tx 1s;
    opacity: 1;
}
.gouwu_texiao_body {
    width: 40px;
    height: 40px;
    position: relative;
}
.gouwu_texiao_body img {
    width: 54px;
    left: -7px;
    top: -7px;
}
@keyframes tx {
    from {
        opacity: 1;
        transform: scale(1);
    }
    to {
        opacity: 0;
        transform: scale(1.5);
    }
}
@-webkit-keyframes tx {
    from {
        opacity: 1;
        -webkit-transform: scale(1);
    }
    to {
        opacity: 0;
        -webkit-transform: scale(1.5);
    }
}
.lover_body {
    position: absolute;
    bottom: 40px;
    right: 0px;
    width: 40%;
}
@-webkit-keyframes zoomInLeft {
    from {
        opacity: 0;
        -webkit-transform: scale(.3);
        transform: scale(.3);
    }
    50% {
        opacity: 1;
    }
}
@keyframes zoomInLeft {
    from {
        opacity: 0;
        -webkit-transform: scale(.3);
        transform: scale(.3);
    }
    50% {
        opacity: 1;
    }
}
.zoomInLeft {
    -webkit-animation: zoomInLeft .5s forwards;
    animation: zoomInLeft .5s forwards;
    transform-origin: left bottom;
    -webkit-transform-origin: left bottom;
}
@-webkit-keyframes zoomOutLeft {
    from {
        opacity: 1;
    }
    50% {
        opacity: 0;
        -webkit-transform: scale(.3);
        transform: scale(.3);
    }
    to {
        opacity: 0;
    }
}
@keyframes zoomOutLeft {
    from {
        opacity: 1;
    }
    50% {
        opacity: 0;
        -webkit-transform: scale(.3);
        transform: scale(.3);
    }
    to {
        opacity: 0;
    }
}
.zoomOutLeft {
    -webkit-animation: zoomOutLeft .5s;
    animation: zoomOutLeft .5s;
    transform-origin: left bottom;
    -webkit-transform-origin: left bottom;
}
.zhuan {
    animation: zhuan .5s;
    -webkit-animation: zhuan .5s;
}
@keyframes zhuan {
    0%, 50%, 100% {
        transform: rotate(0deg);
    }
    25% {
        transform: rotate(-15deg);
    }
    75% {
        transform: rotate(15deg);
    }
}
@-webkit-keyframes zhuan {
    0%, 50%, 100% {
        -webkit-transform: rotate(0deg);
    }
    25% {
        -webkit-transform: rotate(-15deg);
    }
    75% {
        -webkit-transform: rotate(15deg);
    }
}
