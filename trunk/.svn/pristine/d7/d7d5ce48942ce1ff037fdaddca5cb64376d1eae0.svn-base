<script src="scripts/lazyload.js"></script>
<script>


var a=640/200;
$(document).ready(function(e) {
    $('.page_loading').remove();
	$('.item').height($('.item').width()/a);
	$('.list').height($('.item').height()*3.2);
	var tmptime = new Date('2016/01/23');
	var curtime = new Date();
	var enddate =new Date();
	if (  curtime.getTime() < tmptime.getTime()){
		curtime = tmptime;
	}
	if (curtime.getHours() >22 ){
		curtime.setHours(0);
		curtime.setDate(curtime.getDate()+1);
	}
	enddate.setDate(curtime.getDate()+1);
	$("img.lazy").lazyload({         
		 container: $(".list")
	});
	$('.item').on('click',function(){
		$('input[name="startdate"]').val(curtime);
		$('input[name="enddate"]').val(enddate);
		$('input[name="city"]').val($('.local span',this).html());
		$('#form').submit();
	})
});
</script>
