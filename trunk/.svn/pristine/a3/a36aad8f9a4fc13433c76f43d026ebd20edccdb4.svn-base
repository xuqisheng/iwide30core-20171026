.wrapper-verifie .btn{display:inline-block;width:70px;text-align:center;cursor:pointer;padding-top:2px;padding-bottom:2px;padding-left:0px;padding-right:0px;}

/*预约核销*/
.diycheckbox{width:14px;height:14px;display:inline-block;border-radius:3px;border:1px solid #c7c7c7;}
input[type=checkbox]:checked+label .diycheckbox{border:0;}
input[type=checkbox]:checked+label .diycheckbox:before{content:'\2714';color:#fff;background:#b69b69;font-size:13px;height:14px;width:14px;display:inline-block;vertical-align:initial;text-align:center;}
.diyradio{width:14px;height:14px;display:inline-block;border-radius:50%;padding:2px;border:1px solid #dae0ea;font-size:0;margin-right:5px;}
.diyradio tt{height:8px;width:8px;display:inline-block;border-radius:50%;}
input[type=radio]:checked+label .diyradio{border-color:#4caf50;}
input[type=radio]:checked+label .diyradio tt{background:#4caf50;}
.specifications:hover i.iconfonts{display:inline-block;}
.add_img{width:120px;height:120px;background:#f6f6f6 url('../img/imgadd.jpg') no-repeat;}
.time_selects_btn[name='sale_now']:checked+label .time_selects{display:inline-block;}
.date_select,.date_select td,.date_select th{border:1px solid #e7e7e7;}
.date_select thead th{text-align:center;height:45px;border:none;}
.date_select tr:nth-of-type(1)  td{border:none;border-top:1px solid #e7e7e7;text-align:center;height:30px;}
.date_select td{cursor:pointer;}
.date_select td p:nth-of-type(1){text-align:right;padding-right:10px;padding-top:10px;}
.date_select td p:nth-of-type(2){text-align:center;font-size:14px;}
.date_txt{margin:0px 90px;}
.date_select .d_last,.date_select .d_next{width:23px;height:23px;line-height:21px;margin:auto;display:inline-block;text-align:center;}
/*预约核销弹窗*/
.p_fixed{position:fixed;top:0px;left:0px;width:100%;height:100%;background:rgba(0,0,0,0.5);z-index:9999}
.elastic_con_1{margin:auto;width:650px;}
.p_fixed .close_btn{top:25px;right:25px;}



