.j_form th{text-align:center;}
.j_form th,.j_form td{padding:18px 0;}
.j_form tbody tr:nth-child(even){background:#f8fafc;}
.j_form tbody tr:hover{background:#fafafa;cursor:pointer;}
table.j_form thead .sorting:after,table.j_form thead .sorting_asc:after,table.j_form thead .sorting_desc:after{position:initial;display:inline-block;vertical-align:middle;margin-left:10px;}
.j_form .row:nth-of-type(1){display:none;}
.j_form li.active a{background:#b69b69 !important;border-color:#b69b69 !important;}
.date_ico{right:5px;top:-4px;font-size:20px;}
.form_head input{font-size:12px;color:#333;}

