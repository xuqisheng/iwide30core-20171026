* {
  margin: 0;
  padding: 0;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.f28 {
  font-size: 28/2px;
}

.f24 {
  font-size: 24/2px;
}

.f32 {
  font-size: 32/2px;
}

.c333 {
  color: #333333;
}

a {
  text-decoration: none;
}

li {
  list-style: none;
}

.clearfix {
  &:after {
    content: "";
    height: 0;
    clear: both;
    overflow: hidden;
  }
}

input, select {
  border: none;
  outline: none;
  -webkit-appearance: none;
  -moz-appearance: none;
  -ms-appearance: none;
  font-size: 24/2px;
  padding: 0 18/2px;
  vertical-align: top;
  border-radius: 4px;
}

select {
  background: url("data:image/png;base64,") no-repeat scroll right center transparent;
  background-size: 10px;
  background-position-x: 90%;
}

input[type="radio"] {
  width: 28/2px !important;
  height: 28/2px !important;
  padding: 0px !important;
  border: 1px solid #e1e1e1;
  border-radius: 100%;
  vertical-align: middle;
  cursor: pointer;
  position: relative;
  box-shadow: none;
}

input[type="radio"]:checked {
  border: 1px solid #b69b69;
}

input[type="radio"]:checked::after {
  content: "";
  position: absolute;
  width: 50%;
  height: 50%;
  left: 25%;
  top: 25%;
  background: #b69b69;
  border-radius: 100%;
}

input[type="checkbox"] {
  width: 32/2px !important;
  height: 31/2px !important;
  padding: 0 !important;
  border: 1px solid #e1e1e1;
  border-radius: 0;
  position: relative;
}

input[type="checkbox"]:checked {
  border: 1px solid #b69b69;
}

input[type="checkbox"]:checked::after {
  content: "";
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: url("data:image/png;base64,") center center no-repeat;
  background-size: 100% 100%;
}

input::-webkit-input-placeholder {
  color: #bfbfbf;
}

input:-moz-placeholder {
  color: #bfbfbf;
}

input::-moz-placeholder {
  color: #bfbfbf;
}

input:-ms-input-placeholder {
  color: #bfbfbf;
}

input[type="reset"]::-moz-focus-inner,
input[type="button"]::-moz-focus-inner,
input[type="submit"]::-moz-focus-inner,
input[type="file"] > input[type="button"]::-moz-focus-inner {
  border: none;
  padding: 0;
}

.jedateblue .jedaul li.other, .jedateblue .jedaul li.other .nolunar, .jedateblue .jedaul li.other .lunar {
  color: #bfbfbf !important;
}

.jedateblue .jedaul li.action, .jedateblue .jedaul li.action:hover, .jedateblue .jedaul li.action .lunar {
  background: #b69b69 !important;
}

.jedateblue .jedatebot .jedatebtn span {
  background-color: #b69b69 !important;
  cursor: pointer;
}

.jedateblue .jedatetop {
  background: #b69b69 !important;
}

.jedateblue {
  border: 1px #b69b69 solid !important;
}

.jedateblue .jedatehmstitle {
  background: #b69b69 !important;
}

.jedateblue .jedatehmscon p.action, .jedateblue .jedatehmscon p.action:hover {
  background: #b69b69 !important;
}

.jedateblue .jedateym .triangle:hover {
  background-color: #d8c67c !important;
}

.jedatebox .jedatehmscon::-webkit-scrollbar-thumb {
  background: #bfbfbf !important;
}

.jedatebox .jedatehmscon::-webkit-scrollbar-thumb:hover {
  background: #bfbfbf !important
}

.jedatebox .jedatehmscon::-webkit-scrollbar-thumb:active {
  background: #bfbfbf !important
}

.jedatebox .jedatehmscon::-webkit-scrollbar-thumb:window-inactive {
  background: #bfbfbf !important;
}

.jedatetopym .action, .jedatetopym .jedateymchok {
  background: #b69b69 !important
}

// 首页
.kill-index {
  background: #ffffff;
  min-height: 540px;

  // 分类切换
  .tab {
    padding-left: 20/2px;
    padding-right: 30/2px;
    overflow: hidden;
    margin-bottom: 40/2px;

    a:hover, a:active, a:focus {
      color: #333333;
    }

    a {
      width: 222/2px;
      text-align: center;
      padding: 39/2px 0;
      position: relative;

      &.active {
        color: #b69b69;
        &:after {
          position: absolute;
          content: "";
          height: 4/2px;
          width: 100%;
          background: #b2955e;
          left: 0;
          bottom: 0;
        }
      }

      &.add {
        width: 268/2px;
        span {
          height: 66/2px;
          line-height: 66/2px;
          background: #f3f3f3;
          font-size: 28/2px;
          display: block;
          color: #b2955e;
          border: 1px solid #ededed;
          border-radius: 10/2px;
        }
      }
    }
  }

  .container {
    padding: 0 30/2px;
    // 搜索
    .search {
      overflow: hidden;
      box-sizing: content-box;
      background: #f3f3f3;
      padding: ((96-56)/2)/2px;
      height: 56/2px;
      line-height: 56/2px;

      .page-title {
        margin-left: 70-35px;
        margin-right: 30/2px;
      }

      .select-wrap {
        background: #ffffff;
        width: 67px;
        overflow: hidden;
        border-radius: 8/2px;
      }

      .select {
        width: 134/2px;
        height: 56/2px;
        line-height: 56/2px;
        margin-right: 140/2px;
      }

      .key {
        height: 56/2px;
        line-height: 56/2px;
        background: #ffffff;
        border-radius: 4px;
        width: 406/2px;
        overflow: hidden;
        padding: 0 16/2px;

        .input {
          input {
            height: 56/2px;
            line-height: 56/2px;
            width: 318/2px;
          }
        }

        a {
          width: (406-322-16-16)/2px;
          height: 56/2px;
          background: url("data:image/png;base64,") center center no-repeat;
          background-size: 50% 50%;
        }
      }

      .go-search {
        height: 56/2px;
        line-height: 56/2px;
        background: #b69b69;
        margin-left: 24/2px;
        color: #ffffff;
        width: 160/2px;
        vertical-align: top;
        text-align: center;
      }
    }

    // 表格
    .table {
      table {
        width: 100%;
        th {
          padding-top: 72/2px;
          padding-bottom: 28/2px;
          color: #808080;
          font-size: 24/2px;
          text-align: center;
          font-weight: normal;
          border-bottom: 1px solid #ccd6e1;
        }

        .operation {
          text-align: center;
          width: 600/2px;
        }

        tr {
          &:nth-child(2n) {
            background: #ffffff;
          }

          &:nth-child(2n+1) {
            background: #eff2f7;
          }

          &:nth-child(1) {
            background: #ffffff;
          }
        }

        td {
          height: 148/2px;
          vertical-align: middle;
          color: #333333;
          font-size: 24/2px;
          text-align: center;
          padding: 0 40/2px;

          .btn {
            display: inline-block;
            box-sizing: content-box;
            padding: 0;
            height: 56/2px;
            line-height: 56/2px;
            color: #b69b69;
            border: 1px solid #b69b69;
            text-align: center;
            width: 154/2px;
            font-size: 28/2px;
            margin-right: 12/2px;

            &.placeholder {
              visibility: hidden;
            }

            &.disabled {
              border: 1px solid #ffffff;
              color: #333333;
            }

            &:last-child {
              margin-right: 0;
            }
          }

        }
      }
    }

    // 分页
    .pagination {
      height: 48/2px;
      line-height: 48/2px;
      margin-top: 36/2px;
      padding-bottom: 36/2px;
      color: #bfbfbf;
      width: 100%;

      .page-number {
      }

      span {
        color: #808080;
      }

      .page {
        margin-right: 10px;
        a {
          height: 46/2px;
          line-height: 46/2px;
          text-align: center;
        }
        .last, .next, .go {
          width: 46/2px;
          border: 1px solid #eeeeee;
          border-radius: 5/2px;
        }

        .last {
          margin-right: 5px;
          background: url("data:image/png;base64,") center center no-repeat;
          background-size: 9/2px 16/2px;
        }

        .next {
          margin-left: 5px;
          background: url("data:image/png;base64,") center center no-repeat;
          background-size: 9/2px 16/2px;
        }

        .go {
          font-size: 24/2px;
          color: #333333;
        }

        .tips {
          height: 48/2px;
          line-height: 48/2px;
          font-size: 20/2px;
          color: #555555;
          margin-right: 10/2px;
          margin-left: 10/2px;
        }

        .go-to-input {
          width: 48/2px;
          height: 46/2px;
          border: 1px solid #eeeeee;
          padding: 0;
          text-align: center;
          color: #333333;
          margin: 0 10/2px 0 0;
        }

        .current {
          border: 1px solid #eeeeee;
          border-radius: 5/2px;

          a {
            box-sizing: content-box;
            border-right: 1px solid #eeeeee;
            padding: 0 20/2px;
            font-size: 12px;
            color: #808080;

            &.active {
              background: #b2955e;
              color: #ffffff;
            }

            &:last-child {
              border-right: none;
            }
          }
        }

      }

    }
  }
}

// 新建秒杀
.kill-edit {

  position: relative;

  #week, #selectTime, #limitPeopleNumber, #peopleNumberTips, #errorBox, #goodsInfo {
    display: none;
  }

  .error {
    background: #fff4e2;
    height: 30/2px;
    line-height: 30/2px;
    padding: 34/2px 74/2px 34/2px 56/2px;
    border: 1px solid #fde7ba;
    border-radius: 10/2px;
    color: #666;
    margin-bottom: 20/2px;

    .icon {
      width: 30/2px;
      height: 30/2px;
      background: url("data:image/png;base64,") center center no-repeat;
      background-size: 100%;
      margin-right: 40/2px;
    }

    .close {
      width: 30/2px;
      height: 30/2px;
      background: url("data:image/png;base64,") center center no-repeat;
      background-size: 100%;
      position: relative;
      z-index: 999;
    }
  }

  .container {
    padding: 60/2px;
    background: #ffffff;

    // 主标题
    .main-title {
      position: relative;
      margin-bottom: 40/2px;
      h1 {
        display: inline-block;
        color: #a5a5a5;
        font-weight: normal;
        height: 30/2px;
        line-height: 30/2px;
        position: relative;
        padding-right: 26/2px;
        z-index: 999;
        vertical-align: top;
        background: #ffffff;
      }

      .line {
        position: absolute;
        width: 100%;
        top: 0;
        left: 0;
        height: 30/2px;
        &:after {
          position: absolute;
          content: '';
          width: 100%;
          height: 2px;
          left: 0;
          top: 50%;
          margin-top: -1px;
          background: #f7f7f7;
        }
      }
    }

    .kill-content {
      padding: 0 18/2px;
      position: relative;

      .part {
        width: 50%;
      }

      .input {
        height: 60/2px;
        line-height: 60/2px;
        border: 1px solid #e5e5e5;
        vertical-align: top;
      }

      .w200 {
        width: 200/2px;
      }

      .w340 {
        width: 340/2px;
      }

      ul {
        li {
          margin-bottom: 40/2px;
          height: 62/2px;
          line-height: 62/2px;
          .sub-title {
            width: 120/2px;
            text-align: right;
            color: #333333;
            height: 62/2px;
            line-height: 62/2px;
            margin-right: 50/2px;

            &.second-title {
              text-align: left;
              width: 120/2px;
              margin-right: 48/2px;
            }

            &.limit-title {
              width: 60/2px;
            }

            .required {
              color: red;
            }

          }

          .item {
            height: 62/2px;
            line-height: 62/2px;

            input[type="radio"] {
              margin-right: 5px;
            }

            input[type="checkbox"] {
              margin-right: 5px;
            }

            .week, .radio {
              display: inline-flex;
              align-items: center;
              height: 62/2px;
              line-height: 62/2px;
              margin-right: 120/2px;
            }

            .week {
              margin-right: 46/2px;
            }

            .unit {
              height: 62/2px;
              line-height: 62/2px;
              margin-left: 20/2px;
              margin-right: 20/2px;
            }

            .select {
              border: 1px solid #e5e5e5;
              height: 62/2px;
              line-height: 62/2px;
              padding: 0 24/2px;
              width: 558/2px;
              background-position-x: 96%;
            }
          }

          .tips {
            color: #bfbfbf;
          }

          .goods {
            .mr-60 {
              margin-right: 60/2px;
            }

            .mr-30 {
              margin-right: 30/2px;
            }
          }

          #startUpTime {
            background: url("data:image/png;base64,") 96% center no-repeat;
            background-size: 37/2px 37/2px;
          }

          .formdate {
            height: 60/2px;
            line-height: 60/2px;
            border: 1px solid #e5e5e5;
            padding-right: 30px !important;
            background: url('data:image/png;base64,') 96% center no-repeat;

            &.start-time {
              width: 240/2px;
              margin-right: 74/2px;
            }

          }
        }
      }

      .save {
        text-align: center;
        a {
          display: inline-block;
          width: 296/2px;
          height: 88/2px;
          line-height: 88/2px;
          color: #808080;
          border-radius: 10/2px;
          border: 1px solid #bfbfbf;
          margin-right: 14/2px;

          &.active {
            background: #b69b69;
            border: 1px solid #b69b69;
            color: #ffffff;
          }
        }
      }

    }

  }

  &.disabled {

    &:after {
      content: "";
      position: absolute;
      width: 100%;
      height: 100%;
      top: 0;
      left: 0;
      z-index: 99999;
    }

    input[type="radio"]:checked {
      border: 1px solid #bfbfbf;
    }

    input[type="radio"]:checked::after {
      content: "";
      position: absolute;
      width: 50%;
      height: 50%;
      left: 25%;
      top: 25%;
      background: #bfbfbf;
      border-radius: 100%;
    }

  }

}

// 弹窗
.kill-layer {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .5);

  .layer-content {
    position: absolute;
    min-width: 686/2px;
    border-radius: 10/2px;
    left: 50%;
    top: 50%;
    padding-top: 45/2px;
    padding-bottom: 80/2px;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background: #ffffff;

    .close {
      position: absolute;
      width: 20px;
      height: 20px;
      background: url("data:image/png;base64,") center center no-repeat;
      background-size: 50% 50%;
      opacity: 1;
      right: 49/2px;
      top: 48/2px;
      z-index: 99;
    }

    .title {
      text-align: center;
      position: relative;
    }

    .tips-content {
      padding: 106/2px 80/2px 106/2px 80/2px;
      text-align: center;
    }

    .store {
      margin: 0 auto;
      padding: 80/2px 80/2px 60/2px 80/2px;

      li {
        color: #333333;
        height: 60/2px;
        line-height: 60/2px;
        margin-bottom: 20/2px;

        .title {
          width: 168/2px;
          text-align: right;
          color: #808080;
          margin-right: 50/2px;
        }

        .number {
          height: 58/2px;
          line-height: 58/2px;
          width: 146/2px;
          border: 1px solid #eeeeee;
        }

        .unit {
          height: 60/2px;
          line-height: 60/2px;
          margin-left: 16/2px;
        }

      }
    }

    .btn {
      box-sizing: content-box;
      display: block;
      padding: 0;
      width: 304/2px;
      margin: 0 auto;
      height: 80/2px;
      line-height: 80/2px;
      background: #b69b69;
      color: #ffffff;
      text-align: center;
    }

  }
}