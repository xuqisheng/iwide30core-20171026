body,p,dl,dd,h3,input,ul{
  margin: 0;
}
ul{
  padding: 0;
}
li{
  list-style: none;
}
body{
  font-family: "PingFang SC", Arial, Helvetica, sans-serif;
  overflow: auto;
}
body,html,.box{
  height: 100%;width: 100%;
}
i{font-style: normal;}
a, label, div, span, li, i {
  tap-highlight-color: transparent;
  -webkit-tap-highlight-color: transparent; 
}
a:focus, label:focus, div:focus, span:focus, li:focus, i:focus {
   outline: none;
}
input[type="radio"]{
  -webkit-appearance: none;
  visibility: hidden;
  display: none;
}
.Lcfx {
    zoom:1
}

.Lcfx:before,.Lcfx:after {
    content: "";
    display: table
}

.Lcfx:after {
    clear: both
}
.Ldf{
  display: -webkit-box;
display: -webkit-flex;
display: flex;
-webkit-box-align: center;
-webkit-align-items: center;
        align-items: center;
-webkit-box-pack: center;
-webkit-justify-content: center;
        justify-content: center;
}
.radio1 + .placeholder{
  padding-left: 18px;
  position: relative;
}
.radio1 + .placeholder:before{
  width: 13px;
  height: 13px;
  border: solid 1px #8fc31f;
  border-radius: 2px;
  position: absolute;
  top: 0;
  left: 0;
  content: '';
}
.radio1:checked + .placeholder:after{
  width: 13px;
  height: 13px;
  content: '';
  background-size: 100%;
  background-position: center;
  background-image: url('data:image/svg+xml;base64,');
  top: 0;
  left: 0;
  position: absolute;
  text-align: center;
  line-height: 16px;
  color: #8fc31f;
  font-size: 16px;
}
.radio2 + .placeholder{
  display: inline-block;
  border: solid 3px #8fc31f;
  border-radius: 9px;
  color: #3e3a39;
  line-height: 20px;
  width: 73px;
  text-align: center;
  background: #fff;
  position: relative;
}
.radio2:checked + .placeholder:before,.radio2:checked + .placeholder:after{
  position: absolute;

  right: 0;
  bottom: 0;
}
.radio2:checked + .placeholder:before{
  content: '';
  width: 0;
  height: 0;
  border-width: 8px;
  border-style: dashed solid solid dashed;
  border-color: transparent #8fc31f #8fc31f transparent;
}
.radio2:checked + .placeholder:after{
  content: '';
  width: 12px;
  height: 12px;
  background-size: 100%;
  background-position: center;
  background-image: url('data:image/svg+xml;base64,');
}
.Ldn{display: none;}
.Ldb{display: block;}
.onepx{
  position: relative;
}
.onepx:before,.onepx:after{
  position: absolute;
  content: '';
  background-color: #fff;
}
.onepx.onepx-horizontal:before{
  -webkit-transform: scale(1, 0.5);
  transform: scale(1, 0.5);
  height: 1px;
}
.onepx.onepx-vertical:after{
  -webkit-transform: scale(0.5, 1);
  transform: scale(0.5, 1);
  width: 1px;
}
