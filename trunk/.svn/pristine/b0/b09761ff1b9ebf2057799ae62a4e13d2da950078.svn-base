  text-align: center;
}
.page-index .arrow{
  position: absolute;
  bottom:16px;
  left:0;
  width: 100%;
  text-align: center;
}
.arrow-cont{
    position: relative;
    width: 30px;
    height: 30px;
    display: inline-block;
}
.arrow-cont:after,.arrow-cont:before{
  position: absolute;
  width: 100%;
  height: 100%;
  background: transparent;
  border: solid 3px #8fc31f;
  border-top: 0 none;
  border-left: 0 none;
  content: '';
  left: 0;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}
.arrow-cont:before{
  top: 0;
}
.arrow-cont:after{
  top: 10px;
}
.layer .box:after{
  position: absolute;
  top: 0;
  right: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  background: rgba(0,0,0,0.5);
  content: '';
}
