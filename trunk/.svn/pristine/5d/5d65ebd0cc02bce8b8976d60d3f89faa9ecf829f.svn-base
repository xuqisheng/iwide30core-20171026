@charset "utf-8";
/* CSS Document */
*{margin:0px;padding:0px;}
a{text-decoration:none;}

.box{width:100%;height:65.2rem;background:url(../images/exchange/bg.jpg) no-repeat;background-size:100% 100%;position:relative;top:0px;left:0px;}
.conter{position:absolute;top:12rem;left:6.25%;width:87.5%;min-height:32.5rem;background:#fff;border-radius:1rem;overflow:hidden;background:url(../images/exchange/bg2.jpg) no-repeat;background-size:100% 100%;text-align:center;}

.text_code{margin-top:18.7rem;font-size:1.75rem;}
.in_code{display:block;width:85.7%;height:4.37rem;font-size:1.75rem;text-indent:0.5rem;margin:1rem auto;outline:none;-webkit-appearance: none;}
.btn{width:17.1rem;height:4.37rem;line-height:4.37rem;margin:auto;color:#fff;background:#ff9900;border-radius:16px;font-size:1.75rem;text-align:center;}


.fixed{width:100%;height:100%;position:fixed;top:0px;left:0px;background:rgba(0,0,0,0.8);display:none;}
.f_con,.f_con1{width:78.2%;min-height:14.19rem;background:#fff;border-radius:20px;margin:19.7rem auto;text-align:center;font-size:1.75rem;position:relative;display:none;}
.describe{padding-top:3.8rem;}
.btn_lis{position:absolute;bottom:0px;left:0px;border-top:1px solid #c5c5c5;width:100%;}
.btn_lis> a{display:inline-block;width:46%;padding:4% 0;color:#ff9900;box-sizing:border-box;border-left:1px solid #c5c5c5;}
.btn_lis> a:nth-of-type(1){border-left:none;}

.su_btn{background:#ff9900;color:#fff;border-radius:42px;width:12.3rem;padding:0.68rem;margin:2% auto;}