* {
  margin: 0;
  padding: 0;
  box-sizing: content-box;
}
div,
p {
  line-height: 1;
}
li {
  list-style: none;
}
a {
  text-decoration: none;
}
body {
  background: #efefef;
}
.box {
  display: -webkit-box;
}
.flex1 {
  -webkit-box-flex: 1;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.clearfix:after {
  content: "";
  display: block;
  clear: both;
  height: 0;
  overflow: hidden;
  visibility: hidden;
}
.pr {
  position: relative;
}
.pa {
  position: absolute;
}
.ta-c {
  text-align: center;
}
#index .popup-wrap {
  width: 82%;
}
#index .img-wrap {
  margin: 0 auto;
}
#index.bg {
  position: relative;
  height: 100%;
  min-height: 568px;
  background: url(../images/bg_02.jpg) center center no-repeat;
  background-size: 100% 100%;
  overflow-y: scroll;
}
#index .header {
  width: 30px;
  height: 30px;
  border-radius: 100%;
  top: 12px;
  right: 16px;
  overflow: hidden;
  background: #dcdcdc;
}
#index .header img {
  max-width: 100%;
}
#index .kill-time {
  height: 30px;
  width: 100%;
  text-align: center;
  margin: 42% auto 0 auto;
  opacity: 0;
  z-index: 999;
  vertical-align: bottom;
}
#index .kill-time .time {
  width: 100%;
