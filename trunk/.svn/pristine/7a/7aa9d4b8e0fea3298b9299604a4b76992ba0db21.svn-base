  height: 226px;
}
#index .img-container img {
  border-radius: 100%;
  width: 100%;
  height: 100%;
}
#index .arrow {
  width: 20.5px;
  height: 28px;
  background: #d1d1d1;
  top: 50%;
  margin-top: -11px;
}
#index .left-arrow {
  left: 20px;
  background: url(../images/left_arrow.png) center center no-repeat;
  background-size: contain;
}
#index .left-arrow.active {
  background: url(../images/left_arrow_active.png) center center no-repeat;
  background-size: contain;
}
#index .right-arrow {
  right: 20px;
  background: url(../images/right_arrow.png) center center no-repeat;
  background-size: contain;
}
#index .right-arrow.active {
  background: url(../images/right_arrow_active.png) center center no-repeat;
  background-size: contain;
}
#index .shop-name {
  display: inline-block;
  position: relative;
  height: 36.5px;
  line-height: 36.5px;
  max-width: 90%;
  text-align: center;
  font-size: 16px;
  padding: 0 18px 0 6px;
  color: #333;
  background: url(../images/shop_name.png) center center no-repeat;
  background-size: 100% 36.5px;
  word-break: keep-all;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
#index .price-wrap {
  font-size: 0;
  display: table;
  width: 100%;
}
#index .price {
  display: table-cell;
  height: 17px;
  width: 100%;
  overflow: hidden;
  margin: 0 auto;
  vertical-align: bottom;
}
#index .symbol {
  font-size: 13px;
  color: #333333;
}
#index .discount {
  font-size: 21px;
  color: #333333;
  margin-left: 4px;
}
#index .original {
  font-size: 12px;
  text-decoration: line-through;
  color: #bfbfbf;
  margin-left: 7px;
}
#kill-index {
  position: relative;
}
#kill-index .button-wrap {
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 27px;
}
#kill-index .button-wrap.kill {
  bottom: 27px;
}
#kill-index .button-wrap .button {
  display: inline-block;
  width: 166.5px;
  height: 45px;
  line-height: 45px;
  text-align: center;
  color: #ffffff;
