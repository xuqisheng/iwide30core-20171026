.popup {
  position: fixed;
  display: none;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}
.popup .popup-wrap {
  width: 90%;
  background: #ffffff;
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
  border-radius: 5px;
  overflow: hidden;
}
.popup .popup-wrap .popup-header {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  height: 15px;
}
.popup .popup-wrap .popup-header a {
  margin-right: 15px;
  margin-top: 15px;
  width: 15px;
  height: 15px;
  background: url(../images/close.png) center center no-repeat;
  background-size: contain;
}
