  color: #333333;
}
#index .kill {
  margin-top: -20px;
}
#index .kill-goods {
  width: 226px;
  height: 226px;
  margin: 0 auto;
}
#index .kill-goods.kill-sign:after {
  content: "";
  position: absolute;
  width: 31px;
  height: 53.5px;
  z-index: 9999999999;
  background: url(../images/kill.png) center center no-repeat;
  background-size: contain;
  left: 10px;
  top: 0;
}
#index .img-container {
  width: 226px;
  height: 226px;
  margin: 0 auto;
  overflow: hidden;
  -webkit-mask: url(../images/img_bg.png);
  background-size: contain;
  border-radius: 100%;
}
#index .img-container .swiper-container {
