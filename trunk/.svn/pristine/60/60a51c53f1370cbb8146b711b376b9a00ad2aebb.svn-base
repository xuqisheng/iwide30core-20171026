        /*不保留左侧时的centre样式*/
        .centre { height: 99%;margin: 0 1%;}

         /****************** 步骤模块 Start **********************/
        .flow{width:100%;height:50px; display: -webkit-box;-webkit-box-orient: horizontal;-webkit-box-pack: center;-webkit-box-align: center;padding-top:20px;font-size:12px;}
        .flow .active{width:30px;height:30px;background-color:#000;font-size: 14px;line-height: 30px;}
        .flow_one,.flow_two,.flow_three,.flow_four,.flow_five{width:25px;height:25px;background-color:#e0e0e0;border-radius:15px 15px 15px 15px;text-align:center;}
        .flow span{color:#fff;line-height: 25px;margin-left: 9px;font-size: 12px;}
        .flowinfo{display: -webkit-box;-webkit-box-orient: horizontal;-webkit-box-pack: center;-webkit-box-align: center;padding-bottom:15px;}
        .flowinfo span{margin: 0 68px 0 70px;color:#808080;font-size: 12px;}
        .flowinfo span.active{color:#000;font-size: 14px;}
        .line {width:150px;border:2px solid #E0E0E0;margin: 5px;}
         /****************** 步骤模块 End **********************/
         
        /*不保留左侧时的info样式*/
        .info{width: 100%; height:870px;margin-bottom:20px;}
        @media (min-height: 700px) { .info{height:820px;} } 
