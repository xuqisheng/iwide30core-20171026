         /****************** 分页 Start **********************/
         .pagination{margin-top:20px;padding-right:20px;width:100%;}
        .pagination #pagination-right{float:right;margin-right: 20px;}
        .pagination #pagination-left{float: left; color: #BFBFBF; margin-left: 30px;font-size:12px;}
        .pagination #pagination-left b{color: #8A8A8A;font-weight: normal;padding:4px;}
        .pagination ul{ border:0; margin:0; padding:0; }
        #pagination-right li { border:0; margin:0; padding:0; font-size:10px; list-style:none; /* savers */ float:left; text-align:center;}
        #pagination-right a{margin-right:2px;border: 1px solid #EEEEEE; }
        #pagination-right #pagination-right .next-off  { border:solid 1px #DEDEDE; color:#B69A6A; display:block; float:left; font-weight:bold; margin-right:2px; padding:6px 11px; }
        #pagination-right .next a,#pagination-right .previous a { font-weight:bold; }	
        #pagination-right .active {border: 1px solid #B69A6A;background:#B69A6A; color:#FFFFFF; font-weight:bold; display:block; float:left; padding:6px 11px; /* savers */ margin-right:2px; }
        #pagination-right a:link,#pagination-right a:visited { color:#333333; display:block; float:left; padding:6px 11px; text-decoration:none; }
        #pagination-right a:hover { border:solid 1px #B69A6A; }
        #pagination-right span {font-size:10px;margin:0; color:#555555;background-color:#fff;line-height: 30px;}
        #pagination-right input[type=text] { background-color: #fff; border: 1px solid #EFEFEF;line-height: 23px;color: #333; text-align: center;outline: none; width: 25px;font-size: 12px; height: 26px;}
        #pagination-right input[type=button]{background-color: #fff;border: 1px solid #EFEFEF; width: 30px;margin: 0px; padding: 0px;font-size: 12px;height: 26px;line-height: 26px;}
        
        /****************** 分页 ENd **********************/

        
        
        /****************** checkbox 样式 **********************/
        .chk_1 {  display: none; } 
        .chk_1 + label { background-color: #FFF; border: 1px solid #C1CACA; box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05); padding: 7px;display: inline-block; position: relative;    vertical-align: middle; margin-top: -2px;margin-bottom: 1px; } 
        .chk_1 + label:active { box-shadow: 0 1px 2px rgba(0,0,0,0.05), inset 0px 1px 3px rgba(0,0,0,0.1); } 
