
        html,body{ margin:0px;font-family: "Microsoft YaHei";background-color:#F5F9FC;} 
        .nvaT { width: 99%; position: relative; height: 70px; border: 1px solid #fff;  margin: 0 auto; background-color: #fff }
        /*width: 13%;*/
        .nvaL{width: 200px;height: 96.7%; position: absolute;top: 3%;border: 1px solid #000; background: #000;}
        .nvaTcho{margin-left:300px;margin:35px 0 0 220px;}
        .cho_left {float:left;}
        .cho_right {float: right; margin-right: 1%}
        .cho_right div:nth-child(1){float:left; border: 1px solid #DBDBDB;border-style: none solid none none; margin-right: 20px;height: 20px; }
        .cho_right div:nth-child(2){float:right;}
        .cho_right div:nth-child(1) img{float:left;width:30px;height:30px;border-radius:15px 15px 15px 15px ;}
        .cho_right div:nth-child(2) img{width:20px;height:20px;border-radius:15px 15px 15px 15px ;}
        .cho_right div:nth-child(1) .username{font-size:14px;}
        .cho_right div:nth-child(1) .user{font-size:12px;}
        .cho_right img,span{margin-right:10px;}
        
