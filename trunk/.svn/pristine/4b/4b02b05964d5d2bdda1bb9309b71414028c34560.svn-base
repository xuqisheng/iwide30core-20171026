X {
	border: 1px solid #d7e0f1;
	border-radius: 100%;
	width: 15px;
	height: 15px;
	display: inline-block;
	vertical-align: middle;
	margin-left: 5px;
	position: relative;
	cursor: pointer;
}

X:before {
	content: "";
	position: absolute;
	width: 12px;
	height: 0px;
	border-top: 1px solid #d7e0f1;
	left: 2px;
	top: 7px;
	transform: rotate(45deg);
}

X:after {
	content: "";
	position: absolute;
	width: 12px;
	height: 0px;
	border-top: 1px solid #d7e0f1;
	left: 2px;
	top: 7px;
	transform: rotate(-45deg);
}

