.border_bottom,.border_top{position:relative;}
.border_bottom:before{
	content: '';
	position:absolute;
	bottom:0px;
	left:0;
	border-bottom: 1px solid #e1e1e1;
	-webkit-box-sizing:border-box;
	box-sizing: border-box;
	width: 200%;
	height: 0;
	-webkit-transform:scale(0.5);
	transform:scale(0.5);
	-webkit-transform-origin:left top;
	transform-origin:left bottom;
}
.border_bottom:after{
	height:0px;bottom:0;
}
.border_top:after{
	content:'';
	position:absolute;
	top:0px;
	left:0;
	border-top:1px solid #e1e1e1;
	-webkit-box-sizing:border-box;
	box-sizing:border-box;
	width:200%;
	height:0;
	-webkit-transform:scale(0.5);
	transform:scale(0.5);
	-webkit-transform-origin:left top;
	transform-origin:left top;
}
.box_input:after,.border_right:after{
	content:'';
	position:absolute;
	top:0px;
	right:0;
	border-right:1px solid #e1e1e1;
	-webkit-box-sizing:border-box;
	box-sizing:border-box;
	width:0;
	height:200%;
	-webkit-transform:scale(0.5);
	transform:scale(0.5);
	-webkit-transform-origin:right top;
	transform-origin:right top;
}
