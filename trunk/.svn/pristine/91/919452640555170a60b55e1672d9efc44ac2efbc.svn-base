.gradient_bg{ /*渐变背景*/
min-height:100%;
background: -webkit-linear-gradient(left,rgba(9,9,9,1),rgba(22,22,22,1),rgba(22,22,22,1),rgba(9,9,9,1));
background: linear-gradient(left,rgba(9,9,9,1),rgba(22,22,22,1),rgba(22,22,22,1),rgba(9,9,9,1));}
.gradient_radial_bg{ /*渐变背景*/
min-height:100%;
background: -webkit-radial-gradient(#333333, #2d2d2d, #252525);)
background: radial-gradient(#333333, #2d2d2d, #252525);}
.bg_alpha{  /*渐变透明*/
  background: url("../images/roomlist_bg.png");
  background-size: 100% 100%;
}
/*默认图*/
.default_banner_img{background: url("../images/default_banner.png") no-repeat;background-size: 8.925rem 8.812rem;background-position: center center;height: 20.125rem;display: inline-block;position: absolute;top: 0px;width: 100%;}
/*弹窗*/
.whole_eject{position: fixed;top: 0px;height: 100%;width: 100%;padding: 0.937rem;background-color: rgba(1,1,1,0.9);z-index: 10;}
.whole_all_eject{width: 100%;height: 100%;padding: 3%;z-index: 10;border-radius: 4px;}
.whole_eject_content,.whole_all_eject_content{height: 100%;position: relative;border-radius: 3.5px;}
.whole_eject_small{position: relative;margin-top: 7rem;border-radius: 4px;}
.whole_bottom_small{position: absolute; bottom: 60px; width: 94%; left: 3%;}
.iScrollIndicator{width: 2.5px!important;border: 0px!important;background: #666666!important;right: 0px!important;}
/*评分条*/
.score_bar{ display: block; height: 0.25rem;border-radius: 7px;margin-top: 0.375rem;}
