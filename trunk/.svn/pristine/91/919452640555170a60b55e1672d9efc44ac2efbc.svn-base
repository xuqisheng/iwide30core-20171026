/*正方图*/
.squareimg{ position:relative; padding-bottom:100%; overflow:hidden}
.squareimg img{position:absolute; width:100%; left:0; min-height:100%}

/*列表*/
.list_style em{display:inline-block; text-align:center; line-height:1}
.list_style>*{display: box;display: -webkit-box;display: -moz-box;-webkit-box-align:center;}
.list_style,.bd_list{padding:0 1.07rem;}
.list_style>*,.bd_list>*{padding:1rem 0;position:relative;}
.list_style>*:after,.bd_list>*:after{height: 1px;content: '';width: 100%; border-top: 1px solid #363636;position: absolute;bottom: -1px; right: 0;transform: scaleY(0.5);-webkit-transform: scaleY(0.5);z-index: 10;}
.list_style.noborder>*:last-child:after,.bd_list>*:last-child:after{ border-top:0}

.input_item{white-space:nowrap}
.input_item>*:first-child{min-width:5rem;display:inline-block}

/*radio和checkbox*/
.check{display:inline-block;width:1.1rem;height:1.1rem;font-style:normal;}
.check input[type=radio],.check input[type=checkbox]{display:none}
.check em:after,.check em:after{ content:'\e009';font-family:"iconfont";color: #666666;font-size: 1.02rem;}
.check input[type=radio]:checked+em:after,.check input[type=checkbox]:checked+em:after,.checked .submit_order_check em:after{ content:'\e010';color: #b2945e;}

/*选择控件*/
.on_checked{position: relative;display: inline-block;vertical-align: top;width: 3.275rem;height: 1.837rem;margin-left: 5px;background-color: #ab8e5a;border-radius: 25px;}
.on_checked:after{content: "";position: absolute;width: 1.7rem;height: 1.68rem;right: 0.1rem;top: 0.07rem;border-radius: 25px;background-color: #363636;}
.off_checked{background-color: rgba(0,0,0,0);border: 1px solid #666666;}
.off_checked:after{left: 0px;top: 0.011rem;right: initial;background-color: #ffffff;}
.disable_checked{background-color: rgba(0,0,0,0);border: 1px solid #1f1f1f;}
.disable_checked:after{left: 0px;top: 0.011rem;background-color: #363636;}
/*加减控件*/
.number_control{position: relative;padding: .356rem .542rem;margin-right:.906rem;}
.number_control span{position: relative;z-index: 2;}
.number_control:after{z-index: 1;content: ''; width: 200%; height: 200%; position: absolute; top: 0; left: 0; border: 1px solid #505050; -webkit-transform: scale(0.5,0.5); transform: scale(0.5,0.5); -webkit-transform-origin: top left;border-radius: 4px;}
.number_control_main{width: 3.625rem;display: inline-block;text-align: center;border-radius: 4px;}
.number_control .off{color: #333333;}
/*旋转*/
.rotate180{transform:rotate(-60deg); -webkit-transform:rotate(-60deg);display:inline-block}
.rotate180{transform:rotate(180deg); -webkit-transform:rotate(180deg);display: inline-block;}
/*浮层*/
.load{position:fixed;z-index:999999999;width:100%;height:100%;top:0;left:0;
background:url(../images/loading.gif) no-repeat rgba(0,0,0,0.5) center center;background-size:30px;}

.fixedlayer{ position:fixed; background:rgba(0,0,0,0.7);}
.nonelayer,.successlayer,.fixedlayer{top:0; left:0;width:100%; z-index:555;height:100%;}
.nonelayer,.successlayer{  background:no-repeat center 5em; background-size:auto 5em;  text-align:center;}
.nonelayer>div,.successlayer>div{padding:11em 0 20px 0;}
.nonelayer{background-image:url(../images/none.png)}
.successlayer{background-image:url(../images/success.png)}
.iconload{background:url(../images/loading.gif) no-repeat; background-size:100%; display:inline-block; width:24px; height:24px; margin-left:5px;}

/*  按钮 */
.button,.button2{color:#fff;text-align:center;min-width:7.32rem;display:block;position: relative;height:  2.812rem;line-height:  2.812rem;padding-top: 0.15rem;}
.button:after,.button2:after{content: ''; width: 200%; height: 200%; position: absolute; top: 0; left: 0; border: 1px solid #b2945e; -webkit-transform: scale(0.5,0.5); transform: scale(0.5,0.5); -webkit-transform-origin: top left;}
.button.xs,.button2{display:inline-block;padding:0.48rem 0;letter-spacing:normal;height: auto;line-height: inherit;}
.button2{border-color:transparent; background:#b2945e;}
.button[disabled],.button.disabled,.button2.disabled,.button2[disabled]{ background:#333; color:#666666; border-color:#333}
.button[disabled]:after,.button.disabled:after,.button2.disabled:after,.button2[disabled]:after{border: 0px}
.spacing{letter-spacing:3px;}
/* 边框 */
/*.bd_all,*/
.bd,.bd_top,.bd_bottom,.bd_left,.bd_right,.bd_border{ position:relative;}
.bd:after,.bd_bottom:after,.bd:before,.bd_top:before,.bd_left:before,.bd_right:after{content: "";position: absolute;z-index:10}
.bd:after,.bd_bottom:after,.bd:before,.bd_top:before{border-top:1px solid #363636;left: 0;width: 100%; height:0; -webkit-transform: scaleY(.5); transform: scaleY(.5); }
.bd_left:before,.bd_right:after{border-left:1px solid #363636;top: 0;width: 0; height:100%; -webkit-transform: scaleX(.5); transform: scaleX(.5); }
.bd_border:after{content: ''; width: 200%; height: 200%; position: absolute; top: 0; left: 0; border: 1px solid ; border-radius: 4px; -webkit-transform: scale(0.5,0.5); transform: scale(0.5,0.5); -webkit-transform-origin: top left;}
.bd_color1:after,.bd_color1:before{ border-color:#363636;}
.bd_color2:after,.bd_color2:before{ border-color:#3a3a3a;}
.bd_color3:after,.bd_color3:before{ border-color:#505050;}
.bd0:after{border: 0px!important;}
.bd:after,.bd_bottom:after{ bottom:-1px;}
.bd:before,.bd_top:before{top:-1px;}
.bd_left:before{left:0px}
.bd_right:after{right:0px}

.bd_clear{border: 0px!important;}
/*条件选择*/
.condition{text-align: left;overflow: hidden;}
.condition > div{float: left;position: relative;width: 31%;margin-right: 2.3%;margin-bottom: 10px;border-radius: 4px;padding:0.7rem 0;text-align: center;}
.condition > div:after{content: ''; width: 200%; height: 200%; position: absolute; top: 0; left: 0; border: 1px solid #505050; border-radius: 4px; -webkit-transform: scale(0.5,0.5); transform: scale(0.5,0.5); -webkit-transform-origin: top left;}
.condition span{display: none;position: absolute;top: -3.5px;right: -3.5px;width: 25px;height: 25px;}
.condition .active:after{border:1px solid #b2945e;}
.condition .active span{display: block;}
/* 背景色 */
body,html{width:100%; height:100%; background:#2d3132;/*微信网页背景色*/}
/*固定色*/
.bg_999{background-color:#999;}
.bg_888{background-color:#888;}
.bg_555{background-color:#555;}
.bg_000{background-color:#000;}
.bg_282828{background-color:#282828;}
.bg_090909{background-color:#090909;}
.bg_999,.bg_888,.bg_555,.bg_000{color:#fff}

/*辅助色系*/
.color1_bg{background-color: #ffffff;}
.color2_bg{background-color: #bfbfbf;}
.color3_bg{background-color: #666666;}
.color4_bg{background-color: #333333;}
.color5_bg{background-color: #d6bf95;}
.color5_bg_gradient{background:-webkit-linear-gradient(right,#b3955f,#bca06d,#c9b082,#d5be94);}
.layer_bg{ background-color: #1f1f1f;}

/*主色系*/
