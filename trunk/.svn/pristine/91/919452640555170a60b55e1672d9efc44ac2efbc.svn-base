@charset "utf-8";
/* CSS Document transparent*/

*{margin:0;padding:0;-webkit-tap-highlight-color: rgba(0,0,0,0);-webkit-text-size-adjust:none;-webkit-text-size-adjust:100%;font-family:PingFang SC Regular,Heiti SC,Helvetica Neue,sans-serif;outline:0; box-sizing:border-box; background-clip:padding-box; /* Safari and Chrome */}

input,select,textarea,button,a{ border:none; font-weight:normal; resize:none; background:none;-webkit-appearance:none; color:#000;}

img{border:0; width:100%;}
del{text-decoration:none; position:relative}
del:after{
  position:absolute; top:35%; left:-10%; width:120%;content:' '; height:1px; z-index:10;
  background: -webkit-linear-gradient(right,rgba(111,111,111,0.1),rgba(111,111,111,1),rgba(111,111,111,0.1));
  background: linear-gradient(right, rgba(111,111,111,0.1) ,rgba(111,111,111,1),rgba(111,111,111,0.1)); }
abbr,acronym{border:0;font-variant:normal}
address,caption,cite,code,dfn,em,th,var{font-style:normal;font-weight:500}
ol,ul{list-style:none}
div,p{line-height:1.4}
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {color: #666666; } 
input:-ms-input-placeholder, textarea:-ms-input-placeholder {color: #666666; }
a,a:active,a:visited{text-decoration: none;}
hr{padding:0; height:0.5px; border:none;
  background: -webkit-linear-gradient(right,rgba(126,107,72,0.0),rgba(126,107,72,1),rgba(126,107,72,0.0));
  background: linear-gradient(right, rgba(126,107,72,0.0) ,rgba(126,107,72,1),rgba(126,107,72,0.0)); }
.ui_loadmore{text-align:center; padding:20px 0;}
.ui_loading{background:url(../images/loading.gif) no-repeat; background-size:100%; display:inline-block; width:24px; height:24px; margin-left:5px; vertical-align:text-bottom;}
.scroll{overflow:auto;-webkit-overflow-scrolling:touch}
.logo{text-align: center;padding-bottom: 1.75rem;margin-top: 4.562rem;}
.logo img{height: 2.812rem; width: 15.468rem; }
/*排列*/
.center{text-align:center}
.txt_l{text-align: left;}
.txt_r{text-align:right}
.float{float:left;}
.floatr{float:right;}
.clear{clear:both}
.static{position:static !important;}
.relative{position:relative}
.absolute{position:absolute;}
.topfixed{position:fixed; top:0; width:100%;z-index:90;}
/*底部*/
.bottomfixed,.fixed_btn{position:fixed; z-index:90;width:100%;left:0;  bottom:0}
.fixed_btn{display: box;display: -webkit-box;display: -moz-box;-webkit-box-align:center}

/*loading*/
.pageloading,.pull_loading{position:fixed;height:100%;z-index:999999999;width:100%;top:0;left:0;text-align:center; background:rgba(0,0,0,0.4)}
.pageloading p,.pull_loading p{margin-top:100px;display:inline-block;font-size:14px}
.pageloading p{border-radius:8px; color:#888; background:rgba(0,0,0,0.5); font-size:10px}
.pageloading p.isload,.pull_loading p{padding:40px 10px 10px 10px;min-width:150px; max-width:150px;background:url(../images/loading.gif) no-repeat; background-position:center 15px;background-size:50px;}
.pageloading p.error{width:auto; padding:4%;}
/*文字裁剪*/
.txtclip{white-space:nowrap}
.txtclip,.multiclip{ word-break:break-all;overflow : hidden;text-overflow: ellipsis; }
.multiclip{display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}
.clip_str>*{width:1em; overflow:hidden;display: inline-block;vertical-align:baseline;white-space:nowrap}
.bold{font-weight:bold;}

.hide{display:none !important}
.overflow{overflow:hidden}
.overscroll{overflow-y:scroll}
.disable,[disable],.gray{-webkit-filter: grayscale(100%);filter: grayscale(100%);opacity:0.9;}

/*盒子*/
.block{display:block}
.inblock{display:inline-block}
.flex{display:-webkit-box;-webkit-box-align:center;display:flex;align-items:center;}
.flexwrap{flex-wrap:wrap;}
.flexgrow,.boxflex>*{-webkit-box-flex: 1;-moz-box-flex:1;box-flex:1;flex-grow: 1; display:block;}
.flexrow{-webkit-box-orient:vertical;-webkit-box-direction:normal;flex-flow:column;-webkit-box-pack:justify;justify-content:space-between }
.flexjustify{-webkit-box-pack:justify;justify-content:space-between;}
.webkitbox{display:-webkit-box;-webkit-box-align:center}
.webkitright{ -webkit-box-pack:end;}
.webkittop{ -webkit-box-align:baseline; align-items:baseline;}
.page{height:100%; display:-webkit-box; display:-webkit-flex; display:-ms-flexbox; display:flex; -webkit-box-align:stretch; -webkit-align-items:stretch; -ms-flex-align:stretch; align-items:stretch; -webkit-flex-flow:column; -ms-flex-flow:column; flex-flow:column;}
.page header,.page footer{-webkit-flex-shrink:0;-ms-flex-negative:0;flex-shrink:0}
.flextop{align-items: flex-start;-webkit-box-align: start;}

.y:before{content:"¥";font-size:0.857rem}
.border_radius{border-radius: 4px;}

.icon,.maxicon{display:inline-block;border-radius:50%; margin-right:0.36rem; text-align:center}
.icon{width:1.428rem; height:1.428rem;line-height:1.428rem; }
.maxicon{font-size:3rem;width:4rem; height:4rem; line-height:4rem}

/*间距*/
.w100{width:100%}
.w80{width:80%}
.w60{width:60%}
.w40{width:40%}
.pad20{ padding:0.609rem;}

.formlist>*{padding:1.07rem 0}

/*段落*/
.paragraph{text-indent:2rem;}
.lineheight17{line-height: 1.7}

