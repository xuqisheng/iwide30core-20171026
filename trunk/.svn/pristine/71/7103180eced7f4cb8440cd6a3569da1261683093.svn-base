.nav{width:94%;padding:0 3%;background:#fff;}
.state{width:100%;display:-webkit-box;position:relative;}
.lis_sta{width:18%;-webkit-box-flex:1;padding:6% 0;}
.lis_sta > div{width:1.4rem;height:1.4rem;color:#fff;border-radius:50%;text-align:center;line-height:1.4rem;
margin:auto;background:#d21527;}
.lis_sta p{color:#999;margin-top:5%;font-size:0.5rem;text-align:center;}
.lis_sta .col{color:#d40f20;}
.lis_sta .bg{background:url(../images/ico_10.png) no-repeat center;background-size:100%;}
.yello{height:0.1rem;background:#d40f20;width:19%;position:absolute;top:40%;left:24%;}
.c_99{height:0.1rem;background:#999;width:19%;position:absolute;top:40%;left:57%;}
.titl{margin-top:3%;background:#e4e4e4;width:94%;padding:1% 3%;color:#525252;font-size:0.6rem;}
.box{width:97%;padding:0 0 0 3%;background:#fff;border-bottom:1px solid #e4e4e4;}
.selsct{border-bottom:1px solid #e4e4e4;padding:4% 0;position:relative;}
.selsct:last-child{border:0;}
.on{background: url(../images/sta_3.png) no-repeat center right;background-position: 96% 40%;background-size: auto 80%;}
.b_sising{box-sizing:border-box;-webkit-box-sizing:border-box;}
.selsct .front{display:inline-block;width:19%;padding-right:2%;}
.selsct .department,.selsct .hotel,.txt_in1,.txt_in2,.txt_in3{width:75%;display:inline-block;}
.selsct .department,.selsct .hotel{background: url(../images/arro_1.png) no-repeat center right;background-position: 96% 40%;background-size: auto 50%;}
.selsct .floa{background:#d21527;color:#fff;padding:4% 2%;position:absolute;right:0;top:0px;border:1px solid #e4e4e4;}
.treaty{width:97%;padding:4% 0 4% 3%;}
.treaty > a{color:#4896e5;}
.treaty .i_no{padding-left:6%;background:url(../images/btn_o.png) no-repeat left center;background-size:27%;}
.treaty .i_ge{background:url(../images/btn.png) no-repeat left center;background-size:23%;}
.floot{width:100%;}
.submit_b{display:block;width:93%;margin:4% auto;color:#fff;background:#d21527;text-align:center;padding:5% 0;border-radius:0.2rem;}