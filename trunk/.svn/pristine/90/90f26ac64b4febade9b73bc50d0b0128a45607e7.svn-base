@charset "utf-8";   
/* CSS Document */

.main_line{height: 4.8rem;width:3rem;margin: 0px 1rem;background: url("../images/line_01.png") no-repeat;background-size: 100% 100%;display: inline-block;opacity: 0.8;}
.shadow_b{position: absolute;bottom: -7px;left: 0px;height: 7px;width: 100%;background: url("../images/line_02.png") no-repeat;background-size: 100% 100%;display: inline-block;}
/*.shadow_b:after{content: "";position: absolute;border-radius: 39%;left: 0px;top: -52px;height: 51px;width: 100%;box-shadow: 0px 0px 6px 1px #b2945e;/* opacity:0.6; */}*/
.shadow_b:before{/*content: "";position: absolute;top: -40px;left: 20%;width: 60%;height: 40px;box-shadow: 0px 0px 3px 0px #b2945e;*/}
.shadow_r{position: absolute;top: -10px;left: 0px;height: 4rem;width: 0.8rem;background: url("../images/line_03.png") no-repeat;background-size: 100% 100%;display: inline-block;}
/*.shadow_r:after{content: "";position: absolute;border-radius: 50%;left: -3px;top: 5px;height: 30px;width: 5px;box-shadow: 3px 0px 6px 0px #b2945e;}*/
.main_shadow_wrap{height: 4.8rem;width: 2.8rem;margin: 0px -0.3rem;background: url("../images/line_04.png") no-repeat;background-size: 100% 100%;display: inline-block;opacity: 0.8;}

.img_gray{-webkit-filter: grayscale(100%); filter: grayscale(100%);}
.wrapper{padding: 15px 0.937rem 0 0.937rem;position: relative;}
.a_line{display: -webkit-box;-webkit-line-clamp: 1;-webkit-box-orient: vertical;overflow: hidden;}
.clearfix:after{content:"";display:block;height:0;clear:both;visibility:hidden;}
.close{position: absolute;top: 0.7125rem;right: 0.875rem;font-size: 1rem;z-index: 2;cursor:pointer;}
/*公共样式*/
.flexlayer{ display:flex; align-items:center; justify-content:space-around}
.flexlayer .layerbox{height:95%; width:95%; border-radius:4px; overflow:hidden;}
.flexlayerBtm{display:flex;flex-flow:column; align-items:center;justify-content:flex-end}
.flexlayerBtm .layerbox{width:100%; max-height:50%;overflow:auto;-webkit-overflow-scrolling:touch}
.iconfont *{ font-family:inherit}

/*我的订单*/
.order_rows{overflow: hidden;margin-top: 0.8rem;position: relative;padding: 0 1px 2.5rem 1.25rem;border-bottom: .5px solid #363636;}
.order_rows:first-child{ margin-top:0;}
.order_rows:last-child{ border: 0px;padding-bottom: 0px;}
.order_list_payment{overflow: hidden;margin-top: 0.6rem;}
.order_payment_time{line-height: 2.3;}
.order_payment_word{line-height: 1.3;padding-right: 15%;}
.order_operation{float: right;line-height: 2.6;}
.order_operation_line{margin: 0px 20px;width: 1px;height: 9px;background-color: #3a3a3a;display: inline-block;}
.my_order_name{margin: 1.518rem 0 1.112rem 0;}
.my_order_time{margin-bottom: 1.25rem;}
/*套餐详情*/
.main_tab{padding: 0.48rem 0 1.2rem 0;display:flex;align-items:center;justify-content:space-around;}
.main_tab_choose{display: inline-block;position: relative;padding: 5px 0;}
#room_package_wrap .main_tab_choose{width: 8rem;}
.package_details .main_tab_choose{padding: 5px 3rem;}
.main_tab_line{border-left:.5px solid #3a3a3a;height: 0.609rem;/*width: 1px;background-color: #3a3a3a;*/display: inline-block;}
.package_word_wrap{margin: 15px 10px 0 10px;position: relative;}
.package_i{background-color: #6e6e6e;border-radius: 50%;width: 18px;height: 18px;text-align: center;line-height: 17px;margin: 3px 5px 0px 0;display: inline-block;}

/*订单详情*/
.img_bg_1{background-image: url('../images/address_bg.png');background-size: 100% 100%;}
.getline{width:5.48rem;min-width:5.48rem;text-align: center;}
.order_information{padding: 30px 0px;margin: 0px 15px;position: relative;}
.coordinate_ico{float: left;height: 30px;}
.light_ico{text-shadow: 0 0 1px #b2945e, 0 0 2px #b2945e;}
.order_details_wrap .shadow_r{width: .5rem;}
.order_details_wrap .main_shadow_wrap{width: 2.6rem;height: 4rem;margin: 0px 1.5rem;opacity: .7;}
.commodity_rosw_title{padding-top: 1.25rem}
.order_details_wrap .my_order_button{height: 1.937rem;line-height: 1.937rem;width: 5.862rem;padding: 0px;min-width: 5.862rem;}
.order_recommend_wrap .swiper-wrapper{height: 13rem}
.order_recommend{width: 8.75rem;padding: 0px;border-radius: 4px;height: 13rem}
.order_recommend del:after{top: 0.55rem;left: 0px;background: -webkit-linear-gradient(right,rgba(196,196,196,0.1),rgba(196,196,196,1),rgba(196,196,196,0.1));background: linear-gradient(right,rgba(196,196,196,0.1),rgba(196,196,196,1),rgba(196,196,196,0.1));}
.order_recommend .squareimg{padding-bottom: 8.437rem;}
.order_recommend .squareimg img{border-top-left-radius: 4px;border-top-right-radius: 4px;}
/*商品列表*/
.commodity_rows{overflow: hidden; position:relative}
/*.commodity_rows .squareimg{padding-bottom: 9.1rem;}*/
.commodity_rows .button{min-width: 6.1rem;height: 1.975rem;padding: 0px;line-height: 1.975rem;}
.commodity_rows .check{ position:absolute; right:10px; top:10px; z-index:10}
.commodity_img_wrap{float: left;width: 132px;position: relative;}
.commodity_img_wrap_zhe{content: "";position: absolute;top: -0.8rem;right: -19%;width: 36%;height: 12rem;background-color: #1f1f1f;-webkit-transform: rotate(15deg);transform: rotate(15deg);}
.commodity_img_wrap .img{overflow: hidden;}
.commodity_img_content{float: right;padding-right:10px;width: calc(100% - 145px);margin-left: 0%;position: relative;}
.number_control{padding: .356rem .3rem;margin-right: .306rem;}
.reduce_room, .add_room{width: 1.775rem;display: inline-block;text-align: center;}
.number_control_main{width: 3.214rem;}
.commodity_details_ico{position: relative;padding-top: 1.1rem;;display: inline-block;margin-left: -30px;}
.commodity_img_wrap .squareimg{padding-bottom: 132px;}
.commodity_img_wrap img{bottom: .5px;}
.goods_info{margin-left: -8px;}
.word_list_left{min-width: 80px;}
.order_detail_title .button_small{float: none;display: inline-block;margin-left: 5px;}
.package_detail{display: none;}
.package_detail_show .package_detail{display: block;}
.package_detail_show .package_detail_ico{    transform: rotate(180deg); -webkit-transform: rotate(180deg);display: inline-block;}
.package_wrapper .check{right: 6px;top: 6px;}
.package_wrapper .number_control{margin: 0px;}
.package_wrapper del:after{top: 9px;left: -1px;height: 0.8px}
.package_select_shadow{height: 3.4rem;width: 0.75rem;}
.order_button{border: .5px solid #b2945e; padding: 1px 15px; line-height: 28px; display: inline-block; text-align: center;margin-right: 6px;}
/*使用套餐*/
.usepack_topper{position: relative;height: 60px;}
.usepack_topper img{height: 100%;opacity: 0.7}

.usepack_topper_content{position: absolute;top: 12px;left: 10px;}
.erweima_img{width: 14rem;padding: 10px;background-color: #ffffff;text-align: center;margin: 2rem auto 1rem auto;}
.erweima_img img{width: 100%;}
.usepack_rows{position: relative;margin: 0px 15px;min-height: 50px;}
.usepack_radio{float: right;}
.usepack_word{    width: 40%; display: inline-block;}
#writeoffcode{margin-bottom: 1rem;}

/*搜索结果页*/
.main_tab_xiala{position: absolute;top: 13px;display: inline-block;}
.results_wrapper_rows{position: relative;}
.results_wrapper_rows .squareimg{padding-bottom: 200px;}
.results_score{color: white;background-color: #9c8459;padding: 1px 6px; border-radius: 3px;margin-top: 0px;height: 1.2rem; line-height: 1.2rem;font-size:0.731rem;}
/*.results_score>*:first-child{font-size:0.914rem;}*/
.results_address{ width: calc(100% - 120px);}
.results_wrapper_rows .imgtitle{padding: 65px 12px 11px 12px;background: -webkit-linear-gradient(bottom,rgba(31,31,31,0.94) 12%,rgba(31,31,31,0) 100%);background: linear-gradient(bottom, rgba(31,31,31,0.94)  12%,rgba(31,31,31,0) 100%);}
.results_address_line{border-left: 0.5px solid #595959;height: 0.509rem;margin-top: 4px;}
.results_identification{position: absolute;top: -5px;left: 12px;color: white;background-color: rgba(51,87,130,0.9);line-height: 3.212rem;min-width: 3.125rem;text-align: center;border-top-right-radius: 13px;border-bottom-left-radius: 13px;z-index: 2;}
.search_results_wrapper em{display: inline-block;width: 2px;height: 15px;}
.search_results_sort em:before{content:"\e022";font-family:iconfont;font-size: 0.914rem;}
.rise em:before{content:"\e023";font-family:iconfont;font-size: 0.914rem;}
.search_results_sort em,.search_results_sort .shadow_b{display: none;}
.sort_Active{color: #ffffff;}
.sort_Active em,.sort_Active .shadow_b{display: inline-block;}
.results_search_left{width: 35%;}
.results_search_left .shadow_b{display: none;}
.results_search_left p{position: relative;padding: 0.5rem 0 0.5rem 0;margin: 0 1rem 2.5rem 1rem;text-align: center;}
.results_search_active span{color: #ffffff;}
.results_search_active .shadow_b{display: block;}
.results_search_right{width: 65%;}
.results_search_right em{display: none;}
.results_search_right p{padding-bottom: 0.8rem;margin-top: 0.9rem;margin-bottom: 0.2rem;}
.results_search_left .active .shadow_b,.results_search_right .active em{display: block;margin-right: 5px;}
.results_search_left .active,.results_search_right .active {color: #ffffff;}
#region_search{height: 22rem;overflow: hidden;}
.hotel_list_letter{display: block;}
.hotel_list_letter:first-letter{font-size:0.914rem;display: block;}
.hotel_list_ico{float: left;margin-top: 11.5px;margin-right: 6px;}
.hotel_list_qi{    margin-top: 20px; margin-left: 6px; float: right;}
/*选择城市*/
.city_wrapper{display: none;/*padding: 0 1.375rem 0 0.937rem;*/position: absolute;top: 0px;left: 0px;width: 100%;height: 100%;z-index: 10;}
.city_search{padding: 30px 1.375rem 27px 0.937rem;}
.nearby_sarch_wrap{padding: 25px 0 18px 0;}
.city_content{padding: 0px  1.375rem 0 0.937rem;position: relative;}
.city_light_ico{text-shadow: 0 0 24px rgba(178,148,94,.7);}
#nearby_sarch{padding-top: 3px;}
#city_search_clear{    padding: 3px; background-color: #666666; border-radius: 50%;color: black;}
.city_search_history > div{display: inline-block;border: 1px solid #505050;padding: 4px 12px;border-radius: 3px;margin: 0 .259rem 10px 0;}
.city_search_hot{overflow: hidden;}
.city_search_hot .swiper-wrapper{padding-right: 2rem;}
.city_search_hot .swiper-slide{position: relative;height: 4.585rem;width: 5.675rem;border-radius: 5px;margin-right: .509rem;}
.city_search_hot img{width: 100%;height: 100%;border-radius: 5px;}
.city_search_hot p{position: absolute;top: 0px;width: 100%;height: 100%;line-height: 4.585rem;text-align: center;background-color: rgba(0,0,0,.5);border-radius: 5px;letter-spacing: 8px;text-indent: 8px;}
.city_search_ico{display: inline-block;margin-top: 2px;}
#serach_whole #region_search > div{padding-bottom: 1rem;}
.city_list_wrap p{padding: 1.07rem 0;border-bottom: 0.5px solid #363636;}
/*.city_list_wrap p:last-child{border: 0px;}*/
.city_letter_list{position: fixed;right: 0px;top: 110px;padding: 10px 0;width: 20px;text-align: center;background-color: rgba(0, 0, 0,1);z-index: 2;}
.city_result_rows{padding: 1.07rem 0 1.07rem 15px;border-bottom: 0.5px solid #363636;overflow: hidden; letter-spacing: 0.5px;}
/*.city_result_rows:after{content: ''; width: 200%; height: 200%; position: absolute; top: 0; left: 0; border: 1px solid #b2945e; border-radius: 4px; -webkit-transform: scale(0.5,0.5); transform: scale(0.5,0.5); -webkit-transform-origin: top left;}*/
.city_result_rows > *{display: inline;}
.city_result_rows .iconfont{width: 13px;height: 16px;margin-top: 1px;float: left;}
.city_no_search_word{    padding: 4rem 0;}

/*发表评论*/
.comment_score_rows{text-align: right;margin-bottom: 2rem;}
.comment_score_rows p{letter-spacing: 10px;}
.comment_score_rows em{display: inline-block;margin-left: 1.2rem;margin-right: 0px;float: left;}
.comment_publish_text{width: 100%;line-height: 1.5rem;}
.comment_publish_text::-webkit-input-placeholder:after{display:block; content:"留个脚印吧~ \A line#"; color:#808080; };
.comment_publish_img{overflow: hidden;}
.comment_publish_img div{position: relative;width: 4.875rem;margin-right: 0.4rem;height: 4.875rem;float: left;border-radius: 5px;margin-bottom: 0.5rem;}
.comment_publish_img img{width: 100%;height: 100%;border-radius: 5px;}
.comment_publish_img p{position: absolute;top: 0px;width: 100%;height: 100%;line-height: 5rem;text-align: center;background-color: rgba(0,0,0,.5);border-radius: 5px;}
.comment_publish_img em{display: inline-block;width: 15px;height: 15px}
.comment_publish_photo{display: block;text-align: center;padding-top: 2rem;display: block; text-align: center; padding: 1rem 0 0.2rem 0;}
.comment_publish_photo_word{display: block;text-align: center;}
.comment_publish_img .comment_publish_img_border{border:.1px dashed #808080;margin-right: 0px;}
#business_val{border-bottom: 1px solid #363636;width: 100%;padding: 0 0 8px 0px;margin-top: 4rem;border-radius: 0px;}

/*评论列表*/
.comment_score{font-size:2rem;/*margin-top: .9rem;*/}
.comment_score:first-letter{font-size:2.5rem;display: block;}
.comment_score:after{content: "分";font-size: 1rem;margin-left: 3px;}
.comment_star em{margin-right: 1.1rem;}
.comment_score{overflow: hidden;}
.comment_score > span{float: left;}
.comment_head{overflow: hidden;padding:0 0 2.3rem 40px;border-bottom: .5px solid #363636;}
.comment_type{position: relative;float: left;padding-bottom: 5px;margin:0 1.8rem 15px 0;}
.comment_type .shadow_b:after{box-shadow: 0px 1px 6px 2px #b2945e;border-radius: 35%;top: -53px;display: none;}
.comment_type .shadow_b{display: none;}
.comment_type_active span{color: #ffffff;}
.comment_type_active .shadow_b{display: block;width: 120%; left: -8%;}
.comment_portrait img{width: 2.25rem;height: 2.25rem;border-radius: 50%;}
.comment_list_score:after{content:"分"; font-size:0.548rem;margin-left: 2px;}
.comment_list_score:first-letter{font-size:1.036rem}
.comment_list_score{font-size:0.731rem}
.comment_list_word{letter-spacing:1px;/*max-height: 60px;*/overflow: hidden;}
.comment_list_hide,.comment_list_all .comment_list_show,.comment_hotel_hide,.comment_hotel_content,.comment_hotel_all .comment_hotel_show{display: none;}
.comment_list_all .comment_list_hide,.comment_hotel_all .comment_hotel_hide{display: block;}
.comment_list_all .comment_list_word{max-height: initial;}
.comment_hotel_all .comment_hotel_content{display: block;}

.comment_list_img,.comment_list_bigimg{overflow: hidden;}
.comment_list_img .squareimg{float: left;padding-bottom: 8.375rem;width: 8.125rem;margin-right: 0.609rem;border-radius: 6px;}
.comment_list_bigimg .squareimg{float: left;padding-bottom: 11.875rem;width: 14.285rem;margin-right: 0.6rem;border-radius: 6px;}
.comment_light_gou{text-shadow: 0 0 2px #b2945e, 0 0 30px #b2945e;}
#add_publish_comment em{margin-left: 1.75rem; }
#comment_success .whole_eject_small{padding: 2.438rem 1.687rem 2.165rem 1.687rem; }
/*选择优惠券*/
.select_coupon_open{position: absolute;bottom: -1.5rem;;right: 0px;width: 100%;text-align: center;}
.select_coupon_bg{height: 8.187rem;width: 10rem;line-height: 8.187rem;text-align: center;padding-right: 2rem;overflow: hidden;}
.select_coupon_money{font-size: 2.5rem;}
.select_coupon_seless .check{width: 3rem;}
.select_coupon_seless *{color: #505050;}
.coupon_shadow .shadow_o:after{   }
.select_coupon_ico{float: left;    margin: 5px -4px 0 0.5rem;}
.select_coupon_word{height: 7.3rem;overflow: hidden;margin-bottom: 1rem;}
.select_coupon_show .coupon_open_show,.coupon_open_hide{display: none;}
.select_coupon_show .coupon_open_hide{display: inline-block;}
.select_coupon_show .rotate180{transform:rotate(0deg); -webkit-transform: rotate(0deg);}
.select_coupon_show .select_coupon_word {height: auto;}
.coupon_shadow{height: 3.3rem;width: 2rem;margin: 0px -1rem;opacity: 1;}
.coupon_wrap .commodity_img_wrap:after{right: -16%;}
@keyframes pulse {
from {transform: scale3d(1, 1, 1); -webkit-transform: scale3d(1, 1, 1); }
50% {transform: scale3d(1.5, 1.5, 1.5); -webkit-transform: scale3d(1.5, 1.5, 1.5); }
to {transform: scale3d(1, 1, 1); -webkit-transform: scale3d(1, 1, 1); }
}
 .pulse {animation-duration: .5s;; animation-fill-mode: both; animation-name: pulse; }

 .hotel_details_wrap .hotel_phone:before{content: ""; position: absolute; z-index: 10; border-left: 1px solid #363636; top: 3px; left: 0px; width: 0; height: 11px; -webkit-transform: scaleX(.5); transform: scaleX(.5);}