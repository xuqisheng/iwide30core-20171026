@charset "utf-8";
/* CSS Document */
*{margin:0;padding:0;-webkit-tap-highlight-color: rgba(0,0,0,0);-webkit-text-size-adjust:none;-webkit-text-size-adjust:100%;font-family: pingfang sc regular,Helvetica Neue,sans-serif;outline:0; box-sizing:border-box;background-clip:padding-box}
body{font-family:pingfang sc regular,Helvetica Neue,sans-serif;}
input,select,textarea,button,a{ border:none; font-weight:normal; resize:none; background:none;-webkit-appearance:none; color:#555;}
table,input{border-collapse:collapse;border-spacing:0}
img{border:0; width:100%;}
abbr,acronym{border:0;font-variant:normal}
del{text-decoration:line-through}
label,input{font-family:pingfang sc regular,Helvetica Neue,sans-serif;}
address,caption,cite,code,dfn,em,th,var{font-style:normal;font-weight:500}
ol,ul{list-style:none}
div,p{line-height:1.5}
.txt_nowrap{text-overflow:ellipsis;white-space:nowrap;overflow: hidden;}
a,a:active,a:visited{text-decoration: none;}
/* 字体大小 */
body,html,input,button,textarea{font-size:14px; /*基础字体大小*/}
@font-face{font-family: 'iconfont';
    src: url('../fonts/iconfont.eot');
    src: url('../fonts/iconfont.eot?#iefix') format('embedded-opentype'),
    url('../fonts/iconfont.woff') format('woff'),
    url('../fonts/iconfont.ttf') format('truetype');
    /*url('../font/iconfont.svg#iconfontyyy') format('svg');*/
}
.iconfont{
  font-family:"iconfont" !important;
 font-style:normal;
  -webkit-font-smoothing: antialiased;
