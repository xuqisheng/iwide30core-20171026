/*登录页面start*/
html,body{width:100%;height:100%;/*min-height:504px;*/background:#f8f8f8;}
.bg_index{width:100%;height:100%;/*background:-webkit-linear-gradient(top,#18dbbd,#23a2cf);background:linear-gradient(top,#18dbbd,#23a2cf);*/}
.logo{width:44%;margin:auto;padding:25% 0 0 0;}
.form{width:80%;margin:25% auto 0;font-size:13px;}
.ico_img{float:left;width:20px;height:20px;vertical-align:middle;margin-right:2%;}
.btn{width:100%;display:inline-block;background:#ff9900;text-align:center;font-size:14px;padding:12px 0;border-radius:2em; box-shadow:0px 10px 10px rgba(255,164,28,0.2)}
.form_list{padding:5px 0;margin:8% 0;}
.con_txt{padding-left:0.3em;}
.threess{padding-left:1.5em;}
.copyright{position:fixed; bottom:0; padding:10px;width:100%; text-align:center}
.copyright img{height:10px; width:auto}
/*登录页面end*/

/*已登录页面start*/
.head{padding:3%;background:#fff;font-size:14px;}
.head_port{float:left;width:35px;height:35px;vertical-align:middle;margin-right:2%;border-radius:50%;overflow:hidden;}
.log_out{float:right;color:#888;}
.log_out,.user_name{padding-top:7px;}
.today_l{background:#fe8f00;font-size:14px;color:#fff;padding:10px 0;}
.long_time{font-size:12px;text-align:center;color:#555;padding:10px 0;}
.parity{width:100%;padding:0 3%;}
.par_con{background:#fff;border-radius:5px;padding:4% 3% 4% 10%;font-size:13px;margin-bottom:4%;}
.par_con:last-child{margin-bottom:0px;}
.right_angle{color:#fff;background:#7f76a2;padding:0px 5px;border-radius:2px;}
.con_number{font-weight:500;font-size:30px;margin-top:1.2%;}
.number{vertical-align:middle;}
.three{color:#888;}
.bottom_box{text-align:center;}
.more_holet{display:inline-block;font-size:12px;text-decoration:underline;color:#7f76a2;padding:10px 0;}
/*已登录页面end*/

/*搜索页面start*/
.head_sea{margin:3%;}
.search_t{width:80%;margin:auto;border:1px solid #e4e4e4;height:30px;border-radius:30px;font-size:13px;}
.sea_input{display:inline-block;line-height:30px;height:30px;text-indent:12px}
.search_btn{font-size:24px;margin-right:5%;margin-top:-2%;color:#888;}
.today_number{padding:10px 4%;font-size:12px;}
.today_number >span:nth-of-type(2){margin-right:5%;}
/*搜索页面end*/


/*搜索结果start*/
.result_con{background:#fff;font-size:13px;}
.hotel_name{padding:5px 4%;}
.ratio{color:#888;}
.lis_titl{padding:5px 0 !important;}
.list_con{padding:8px 0;}
.list_con > div:nth-of-type(1){flex:1.9;-webkit-flex:1.9;text-align:left;}
.list_con > div:nth-of-type(4){text-align:right;}
.list_con > div:nth-of-type(3),.list_con > div:nth-of-type(2){flex:1.4;-webkit-flex:1.4}
/*搜索结果end*/