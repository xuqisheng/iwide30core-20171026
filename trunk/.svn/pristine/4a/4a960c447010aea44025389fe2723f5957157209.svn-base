* {
  margin: 0;
  padding: 0;
}
li {
  list-style: none;
}
a {
  text-decoration: none;
}
body {
  background: #efefef;
}
.box {
  display: -webkit-box;
}
.flex1 {
  -webkit-box-flex: 1;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.pr {
  position: relative;
}
.pa {
  position: absolute;
}
.ta-c {
  text-align: center;
}
#index.bg {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: url(../images/background.png) center center no-repeat;
  background-size: 100% 100%;
  overflow: scroll;
}
#index .header {
  width: 30px;
  height: 30px;
  border-radius: 100%;
  top: 21px;
  right: 16px;
  overflow: hidden;
  background: #dcdcdc;
}
#index .header img {
  max-width: 100%;
}
#index .logo {
  width: 254.5px;
  height: 168px;
  background: url(../images/logo.png) center center no-repeat;
  background-size: contain;
  margin: 0 auto;
}
#index .img-container {
  width: 226px;
  height: 226px;
  margin: -12px auto 0 auto;
  overflow: hidden;
  -webkit-mask: url(../images/img_bg.png);
  background-size: contain;
  border-radius: 100%;
}
#index .img-container .swiper-container {
  width: 226px;
  height: 226px;
}
#index .img-container img {
  border-radius: 100%;
  width: 100%;
  height: 100%;
}
#index .arrow {
  width: 20.5px;
  height: 28px;
  background: #d1d1d1;
  top: 50%;
  margin-top: -11px;
}
#index .left-arrow {
  left: 20px;
  background: url(../images/left_arrow.png) center center no-repeat;
  background-size: contain;
}
#index .left-arrow.active {
  background: url(../images/left_arrow_active.png) center center no-repeat;
  background-size: contain;
}
#index .right-arrow {
  right: 20px;
  background: url(../images/right_arrow.png) center center no-repeat;
  background-size: contain;
}
#index .right-arrow.active {
  background: url(../images/right_arrow_active.png) center center no-repeat;
  background-size: contain;
}
#index .shop-name {
  display: inline-block;
  position: relative;
  height: 36.5px;
  line-height: 36.5px;
  max-width: 90%;
  text-align: center;
  font-size: 16px;
  padding: 0 18px 0 6px;
  color: #333;
  background: url(../images/shop_name.png) center center no-repeat;
  background-size: 100% 36.5px;
  word-break: keep-all;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
#index .price-wrap {
  font-size: 0;
  display: table;
  width: 100%;
}
#index .price {
  display: table-cell;
  height: 17px;
  width: 100%;
  overflow: hidden;
  margin: 0 auto;
  vertical-align: bottom;
}
#index .symbol {
  font-size: 13px;
  color: #333333;
}
#index .discount {
  font-size: 21px;
  color: #333333;
  margin-left: 4px;
}
#index .original {
  font-size: 12px;
  text-decoration: line-through;
  color: #bfbfbf;
  margin-left: 7px;
}
#index .button {
  display: inline-block;
  position: absolute;
  width: 166.5px;
  height: 45px;
  line-height: 45px;
  bottom: 55px;
  left: 50%;
  -webkit-transform: translate(-50%, 0);
  text-align: center;
  color: #ffffff;
  background: url(../images/button.png) center center no-repeat;
  background-size: contain;
}
@media screen and (min-height: 400px) and (max-height: 560px) {
  #index .button {
    position: relative;
    display: block;
    margin-bottom: 31.5px;
    margin-top: 15px;
    bottom: 0;
  }
}
#goodsDetail .tips {
  padding: 13px 15px 7.5px;
  background: #27282d;
}
#goodsDetail .tips .tips-left {
  width: 11px;
  height: 10px;
  background: url(../images/sounds.png) center center no-repeat;
  background-size: contain;
}
#goodsDetail .tips .tips-right {
  color: #ffffff;
  margin-left: 12px;
  vertical-align: top;
  line-height: 20px;
  position: relative;
  top: -3px;
}
#goodsDetail .banner {
  height: 338px;
  background: #dcdcdc;
}
#goodsDetail .detail {
  padding: 16px 15px 0;
  background: #ffffff;
}
#goodsDetail .countdown {
  height: 24px;
  line-height: 24px;
}
#goodsDetail .countdown .countdown-title {
  padding: 0 9px;
  color: #ffffff;
  letter-spacing: 2px;
  background: #fc9327;
}
#goodsDetail .countdown .countdown-content {
  height: 22px;
  line-height: 22px;
  color: #d2944f;
  padding: 0 9px;
  border-top: 1px solid #fdcc90;
  border-bottom: 1px solid #fdcc90;
  border-right: 1px solid #fdcc90;
}
#goodsDetail .title {
  color: #333333;
  margin-top: 15px;
  line-height: 21px;
}
#goodsDetail .number {
  margin-top: 15px;
  line-height: 1;
  color: #808080;
  padding-right: 54px;
}
#goodsDetail .number .sub-title {
  margin-right: 6px;
}
#goodsDetail .number .line {
  border-right: 1px solid #bfbfbf;
  margin: 0 12px;
}
#goodsDetail .number .progress {
  height: 12px;
  margin-left: 10px;
  position: relative;
}
#goodsDetail .number .progress div {
  position: absolute;
  top: 50%;
  margin-top: -3px;
  height: 6px;
  width: 100%;
  background: #fc9327;
}
#goodsDetail .price-wrap {
  margin-top: 20px;
  padding-bottom: 26px;
  border-bottom: 1px solid #efefef;
}
#goodsDetail .price {
  color: #f95948;
  vertical-align: bottom;
  display: table-cell;
  font-size: 0;
}
#goodsDetail .price .symbol {
  margin-right: 6px;
}
#goodsDetail .price .original {
  margin-left: 9px;
  color: #bfbfbf;
  text-decoration: line-through;
}
#goodsDetail .selected {
  padding: 15px 0;
  color: #333333;
  border-bottom: 1px solid #efefef;
}
#goodsDetail .selected .flex1 {
  position: relative;
}
#goodsDetail .selected span {
  color: #808080;
}
.f24 {
  font-size: 12px;
}
.f20 {
  font-size: 10px;
}
.f28 {
  font-size: 14px;
}
.f34 {
  font-size: 17px;
}
.f46 {
  font-size: 23px;
}
