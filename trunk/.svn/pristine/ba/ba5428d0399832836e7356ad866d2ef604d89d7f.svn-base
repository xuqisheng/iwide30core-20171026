.nav{width:94%;padding:0 3%;}
.state{width:100%;display:-webkit-box;position:relative;}
.lis_sta{width:18%;-webkit-box-flex:1;padding:6% 0;}
.lis_sta > div{width:1.4rem;height:1.4rem;color:#fff;border-radius:50%;text-align:center;line-height:1.4rem;
margin:auto;background:#999;}
.lis_sta p{color:#999;margin-top:5%;font-size:0.5rem;text-align:center;}
.lis_sta .col{color:#f9aa31;}
.lis_sta .bg{background:url(../images/ico_10.png) no-repeat center;background-size:100%;}
.yello{height:0.1rem;background:#f9aa31;width:19%;position:absolute;top:40%;left:24%;}
.c_99{height:0.1rem;background:#999;width:19%;position:absolute;top:40%;left:57%;}
.titl{background:#e4e4e4;width:94%;padding:1% 3%;font-size:color:#8e8e8e;font-size:0.6rem;}
.box{width:97%;padding:0 0 0 3%;background:#fff;border-bottom:1px solid #e4e4e4;}
.selsct{border-bottom:1px solid #e4e4e4;padding:4% 0;position:relative;}
.selsct:last-child{border:0;}
.selsct .front{display:inline-block;width:19%;margin-right:2%;}
.box .all_right,.box .error{background-size:auto 100%; background-position: center right; background-repeat:no-repeat;}
.box .all_right{background-image:url(../images/check.png)}
.box .error{background-image:url(../images/sta_3.png)}
.selsct .floa{background:#e4e4e4;color:#a7a7a7;padding:4% 2%;position:absolute;right:0;top:0px;border:1px solid #e4e4e4;}
.selsct .send{background:#fe8f00;color:#fff;border:1px solid #fe8f00;}
.treaty{width:97%;padding:4% 0 4% 3%;}
.treaty > a{color:#4896e5;}
.treaty .i_no{padding-left:6%;background:url(../images/btn_o.png) no-repeat left center;background-size:27%;}
.treaty .i_ge{background:url(../images/btn.png) no-repeat left center;background-size:23%;}
.floot{width:100%;}
.floot p{width:93%;margin:4% auto;color:#fff;background:#fe8f00;text-align:center;padding:5% 0;border-radius:0.2rem;}